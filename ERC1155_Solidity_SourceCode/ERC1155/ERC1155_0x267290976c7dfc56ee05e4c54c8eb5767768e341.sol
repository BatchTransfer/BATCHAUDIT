// File: contracts/nft/MultiKocoonNFT.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Supply.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "../libs/UniversalERC20Upgradeable.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "operator-filter-registry/src/DefaultOperatorFilterer.sol";

contract MultiKocoonNFT is ERC1155Supply,AccessControl, Initializable, ERC2981, DefaultOperatorFilterer {
    using Counters for Counters.Counter;
    using Strings for uint256;
    using UniversalERC20Upgradeable for IERC20Upgradeable;
    
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    /// @notice The NFT Collection Contract Address
    address public factory;
    /// @notice The owner of this contract
    address public owner;
    /// @notice The token address that is used to mint.
    address public mintTokenAddr;
    /// @notice The Royalty Address, which is the owner as default.
    address public royaltyReceiver;
    
    /// @notice NFT Token Base URI
    string public _tokenBaseURI = "";
    /// @notice NFT Token Reveal URI
    string public _revealURI = "";

    /// @notice NFT Token Total Supply
    uint16 public TOTAL_SUPPLY = 10000;
    /// @notice NFT Token Total Supply
    uint16 public ITEM_QUANTITY = 100;
    /// @notice Royalty Percent
    uint16 public royaltyBasisPoints = 300;//3%
    /// @notice the max count of the token that is sold in the presale step.
    uint16 public preCountLimit = 1000;
    /// @notice The max nft count per wallet address in the presale step.
    uint16 public preMaxMintCnt = 1;
    /// @notice The max nft count per wallet address in the public sale step.
    uint16 public pubMaxMintCnt = 10;
    /// @notice The max nft count per transaction
    uint16 public maxMintCntPerTran = 0;

    /// @notice Token Reveal Time
    uint64 public _revealTime;
    /// @notice PreSale Start Time
    uint64 public _preStartTime;
    /// @notice PresSale End Time
    uint64 public _preEndTime;
    /// @notice Public Sale Start Time
    uint64 public _pubStartTime;

    /// @notice Mint Price
    uint256 public PRICE = 0.02 ether;

    bytes32 private _whitelistRoot;
    
    mapping(address => uint16) public _mappingPubMintCount;
    mapping(address => uint16) public _mappingPreMintCount;
    
    Counters.Counter public _publicCounter;

    constructor() ERC1155("MultiKocoon") {
        factory = msg.sender;
    }

    function initialize(
        string memory tokenBaseURI_,
        string memory revealURI_,
        bytes32 merkleroot_,
        uint256 numbers_,
        uint256 timeNumbers_,
        uint256 price_,
        bytes calldata addresses_
    ) external initializer{
        _tokenBaseURI = tokenBaseURI_;
        _revealURI = revealURI_;
        _whitelistRoot = merkleroot_;
        PRICE = price_;
        _initializeNumbers(numbers_);
        _initializeTimeNumbers(timeNumbers_);
        _initializeAddresses(addresses_);
    }

    /// @notice Initialize number props
    /// @param numbers_ TOTAL_SUPPLY|ITEM_QUANTITY|royaltyBasisPoints|preCountLimit|preMaxMintCnt|pubMaxMintCnt|maxMintCntPerTran
    function _initializeNumbers(uint256 numbers_) internal {
        maxMintCntPerTran = uint16(numbers_);
        pubMaxMintCnt = uint16(numbers_ >> 16);
        preMaxMintCnt = uint16(numbers_ >> 32);
        preCountLimit = uint16(numbers_ >> 48);
        royaltyBasisPoints = uint16(numbers_ >> 64);
        ITEM_QUANTITY = uint16(numbers_ >> 80);
        TOTAL_SUPPLY = uint16(numbers_ >> 96);
    }

    /// @notice Initialize number props
    /// @param numbers_ _revealTime|_preStartTime|_preEndTime|_pubStartTime
    function _initializeTimeNumbers(uint256 numbers_) internal {
        _pubStartTime = uint64(numbers_);
        _preEndTime = uint64(numbers_ >> 64);
        _preStartTime = uint64(numbers_ >> 128);
        _revealTime = uint64(numbers_ >> 192);
    }

    /// @notice Initialize address props
    /// @param addresses_ owner|royaltyReceiver|mintTokenAddr
    function _initializeAddresses(bytes memory addresses_) internal {
        // 4 addresses encode packed bytes should be 80 bytes length
        uint16 ADDRESS_LENGTH = 20;
        require(addresses_.length == 20 * 3, "Invalid Address");
        address owner_;
        address royaltyReceiver_;
        address mintTokenAddr_;
        assembly {
            owner_ := mload(add(addresses_, ADDRESS_LENGTH))
            royaltyReceiver_ := mload(add(addresses_, mul(ADDRESS_LENGTH, 2)))
            mintTokenAddr_ := mload(add(addresses_, mul(ADDRESS_LENGTH, 3)))
        }
        require(owner_ != address(0) && royaltyReceiver_ != address(0), "Invalid Zero Address");
        owner = owner_;
        royaltyReceiver = royaltyReceiver_;
        mintTokenAddr = mintTokenAddr_;
    }

    function updateOwner( address newOwner) external onlyOwner {
        owner = newOwner;
        _setupRole(DEFAULT_ADMIN_ROLE, owner);
        _setupRole(MINTER_ROLE, owner);
    }

    function updateDetailInfo(
        bytes32 merkleroot_,
        uint256 numbers_,
        uint256 timeNumbers_,
        uint256 price_,
        bytes calldata addresses_
    ) external onlyOwner {
        _whitelistRoot = merkleroot_;
        PRICE = price_;
        _initializeNumbers(numbers_);
        _initializeTimeNumbers(timeNumbers_);
        _initializeAddresses(addresses_);
    }
    
    function setPresaleLimit(uint16 _preCountLimit) external onlyOwner {
        require(_publicCounter.current() < _preCountLimit, "You must be more than current minted amount.");
        preCountLimit = _preCountLimit;
    }

    function setPubMaxMintCount(uint16 maxLimit) external onlyOwner {
        pubMaxMintCnt= maxLimit;
    }

    function setPreMaxMintCount(uint16 maxLimit) external onlyOwner {
        preMaxMintCnt= maxLimit;
    }

    function setMaxMintCntPerTran(uint16 _maxMintCntPerTran) external onlyOwner {
        maxMintCntPerTran = _maxMintCntPerTran;
    }

    function setMintPrice(uint256 mintPrice, address mintTokenAddr_) external onlyOwner {
        PRICE = mintPrice;
        mintTokenAddr = mintTokenAddr_;
    }

    function setRevealURI(string memory revealURI) external onlyOwner {
        _revealURI = revealURI;
    }

    function setRevealTime(uint64 revealTime) external onlyOwner {
        _revealTime = revealTime;
    }

    function setPreTime(uint64 preStartTime, uint64 preEndTime) external onlyOwner {
        require(preStartTime < preEndTime, "End Time must bigger than Start Time");
        _preStartTime = preStartTime;
        _preEndTime = preEndTime;
    }

    function setPubStartTime(uint64 pubStartTime) external onlyOwner {
        require(_preEndTime < pubStartTime, "The public sale start time must be later than presale end Time");
        _pubStartTime = pubStartTime;
    }

    function setBaseURI(string memory baseURI) external onlyOwner {
        _tokenBaseURI = baseURI;
    }

    function setWhiteListRoot(bytes32 root) external onlyOwner {
        _whitelistRoot = root;
    }

    // Verify that a given leaf is in the tree.
    function isWhiteListed(bytes32 _leafNode, bytes32[] memory proof) public view returns (bool) {
        return MerkleProof.verify(proof, _whitelistRoot, _leafNode);
    }

    // Generate the leaf node (just the hash of tokenID concatenated with the account address)
    function toLeaf(address account, uint256 index, uint256 amount) public pure returns (bytes32) {
        return keccak256(abi.encodePacked(index, account, amount));
    }

    function airdrop(address[] memory airdropAddress, uint256 numberOfTokens) external onlyOwner {
        require(numberOfTokens <= maxMintCntPerTran, "Mint Count per transaction is exceeded");
        require(_publicCounter.current() + numberOfTokens <= TOTAL_SUPPLY,"Purchase would exceed TOTAL_SUPPLY");

        for (uint256 k = 0; k < airdropAddress.length; k++) {
            for (uint256 i = 0; i < numberOfTokens; i++) {
                uint256 tokenId = _publicCounter.current();

                if (_publicCounter.current() < TOTAL_SUPPLY) {
                    _publicCounter.increment();
                    _mint(airdropAddress[k], tokenId, ITEM_QUANTITY, "Mint");
                }
            }
        }
    }

    function purchase(uint16 numberOfTokens) external payable {//Public Sale
        require(hasRole(MINTER_ROLE, msg.sender), "Only minter can add item");
        require(numberOfTokens <= maxMintCntPerTran, "Mint Count per transaction is exceeded");
        require(block.timestamp < _pubStartTime, "Mint is not started yet or already ended.");
        require(_publicCounter.current() + numberOfTokens <= TOTAL_SUPPLY,"Purchase would exceed TOTAL_SUPPLY");
        require(PRICE * numberOfTokens <= msg.value,"ETH amount is not sufficient");
        
        _mappingPubMintCount[msg.sender] = _mappingPubMintCount[msg.sender] + numberOfTokens;
        require(_mappingPubMintCount[msg.sender] <= pubMaxMintCnt, "You reached out up to Limit");
        
        IERC20Upgradeable(mintTokenAddr).universalTransfer(royaltyReceiver, msg.value);

        for (uint256 i = 0; i < numberOfTokens; i++) {
            uint256 tokenId = _publicCounter.current();

            if (_publicCounter.current() < TOTAL_SUPPLY) {
                _publicCounter.increment();
                if(!exists(tokenId))_mint(msg.sender, tokenId, ITEM_QUANTITY, "Mint");
            }
        }
    }
    
    function preSale(uint16 numberOfTokens, uint256 index, uint256 amount, bytes32[] calldata proof) external {
        require(numberOfTokens <= maxMintCntPerTran, "Mint Count per transaction is exceeded");
        require(block.timestamp < _preStartTime, "Presale is not started yet.");
        require(block.timestamp > _preEndTime, "Presale is already ended.");
        require(_publicCounter.current() + numberOfTokens <= preCountLimit,"Purchase would exceed MAX_SUPPLY");
        _mappingPreMintCount[msg.sender] = _mappingPreMintCount[msg.sender] + numberOfTokens;
        require(_mappingPreMintCount[msg.sender] <= preMaxMintCnt, "You reached out up to Limit");

        require(isWhiteListed(toLeaf(msg.sender, index, amount), proof), "You are not registered to WhiteList or Presale");

        for (uint256 i = 0; i < numberOfTokens; i++) {
            uint256 tokenId = _publicCounter.current();

            if (_publicCounter.current() < preCountLimit - 1) {
                _publicCounter.increment();
                if(!exists(tokenId))_mint(msg.sender, tokenId, ITEM_QUANTITY, "Mint");
            }
        }
    }

    function uri(uint256 tokenId) public view override returns (string memory)
    {
        require(exists(tokenId), "ERC1155Tradable#uri: NONEXISTENT_TOKEN");

        if (block.timestamp >= _revealTime)   
            return string(abi.encodePacked(_tokenBaseURI, tokenId.toString()));

        return _revealURI;
    }

    function withdraw() external onlyOwner {
        uint256 balance = address(this).balance;

        payable(msg.sender).transfer(balance);
    }

    modifier onlyOwner() {
        require(owner == msg.sender, "caller is not the owner");
        _;
    }

    // ERC2981 logic
    function updateRoyaltyInfo(address _royaltyReceiver, uint16 _royaltyBasisPoints) external onlyOwner {
        royaltyReceiver = _royaltyReceiver;
        royaltyBasisPoints = _royaltyBasisPoints;
    }
    
    // Takes a _tokenId and _price (in wei) and returns the royalty receiver's address and how much of a royalty the royalty receiver is owed
    function royaltyInfo(uint256 _tokenId, uint256 _price) public view virtual override returns (address receiver, uint256 royaltyAmount) {
        // This contract assumes that all token IDs within the contract have the same royalty info (if that's not an issue, no need to read the rest of this comment)
        // The `_tokenId` param can also be used if you want each token ID to have independent royalty receivers and/or royaltyAmounts
        // (e.g. in the case of an NFT contract being shared between multiple artist, and/or a marketplace)
        // In such cases, you would need to set up a storage space for this data
        // e.g. mapping a uint256 (token ID) to a struct which stores the royalty info for each token
        receiver = royaltyReceiver;
        royaltyAmount = getPercentageOf(_price, royaltyBasisPoints);
    }

    // Represent percentages as basisPoints (i.e. 100% = 10000, 1% = 100)
    function getPercentageOf(
        uint256 _amount,
        uint16 _basisPoints
    ) internal pure returns (uint256 value) {
        value = (_amount * _basisPoints) / 10000;
    }


    /**
     * @dev See {IERC1155-setApprovalForAll}.
     *      In this example the added modifier ensures that the operator is allowed by the OperatorFilterRegistry.
     */
    function setApprovalForAll(address operator, bool approved) public override onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    /**
     * @dev See {IERC1155-safeTransferFrom}.
     *      In this example the added modifier ensures that the operator is allowed by the OperatorFilterRegistry.
     */
    function safeTransferFrom(address from, address to, uint256 tokenId, uint256 amount, bytes memory data)
        public
        override
        onlyAllowedOperator(from)
    {
        super.safeTransferFrom(from, to, tokenId, amount, data);
    }

    /**
     * @dev See {IERC1155-safeBatchTransferFrom}.
     *      In this example the added modifier ensures that the operator is allowed by the OperatorFilterRegistry.
     */
    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public virtual override onlyAllowedOperator(from) {
        super.safeBatchTransferFrom(from, to, ids, amounts, data);
    }

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC1155, ERC2981, AccessControl) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
}



