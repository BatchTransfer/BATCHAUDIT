    function uri(uint256 _tokenid) public view virtual override returns (string memory) {
        return string(abi.encodePacked(super.uri(_tokenid), Strings.toString(_tokenid), ".json"));
    }

    function swapToNFT(uint256 _prize) public {
        uint256 prize = _prize;
        if (prize == bundlePrice.tierOnePrice ) {
            _mint(msg.sender, tierOne, 1, "");
        } else if (prize == bundlePrice.tierTwoPrice ) {
            _mint(msg.sender, tierTwo, 1, "");
        } else if (prize == bundlePrice.tierThreePrice ) {
            _mint(msg.sender, tierThree, 1, "");
        } else if (prize == bundlePrice.tierFourPrice ) {
            _mint(msg.sender, tierFour, 1, "");
        } else if (prize == bundlePrice.tierFivePrice ) {
            _mint(msg.sender, tierFive, 1, "");
        } else if(prize == bundlePrice.tierSixPrice ){
            _mint(msg.sender, tierSix, 1, "");
        } else {
            revert InvalidTier("Invalid Amount");
        }

        //@dev Test scenario
        paymentToken.safeTransferFrom(msg.sender, address(this), prize);

        // if (msg.sender != owner()) {
        //     paymentToken.safeTransferFrom(msg.sender, address(this), prize);
        // }
    }

    function swapToBiome(uint256 _tokenID) external {
        uint256 swapTokenAmount;

        if (_tokenID == tierOne) {
            swapTokenAmount = bundlePrice.tierOnePrice;
        } else if (_tokenID == tierTwo) {
            swapTokenAmount = bundlePrice.tierTwoPrice;
        } else if (_tokenID == tierThree) {
            swapTokenAmount = bundlePrice.tierThreePrice;
        } else if (_tokenID == tierFour) {
            swapTokenAmount = bundlePrice.tierFourPrice;
        } else if (_tokenID == tierFive) {
            swapTokenAmount = bundlePrice.tierFivePrice;
        } else if (_tokenID == tierSix) {
            swapTokenAmount = bundlePrice.tierSixPrice;
        } else {
            revert InvalidTier("Invalid Token ID");
        }

        require(paymentToken.balanceOf(address(this)) >= swapTokenAmount, "Contract lacks tokens");

        paymentToken.safeTransfer(msg.sender, swapTokenAmount);
        _burn(msg.sender, _tokenID, 1);
    }

    function withdrawToken(IERC20 _token) external onlyOwner {
        _token.safeTransfer(msg.sender, _token.balanceOf(address(this)));
    }

    //improve this function
    function setBundlePrice(uint256 bundle, uint256 newPrice) external onlyOwner{
        if(bundle == bundlePrice.tierOnePrice * (10**decimals())){
            bundlePrice.tierOnePrice = newPrice;
        }else if(bundle == bundlePrice.tierTwoPrice * (10**decimals())){
            bundlePrice.tierTwoPrice = newPrice;
        }else if(bundle == bundlePrice.tierThreePrice * (10**decimals())){
            bundlePrice.tierThreePrice = newPrice;
        }else if(bundle == bundlePrice.tierFourPrice * (10**decimals())){
            bundlePrice.tierFourPrice = newPrice;
        }else if(bundle == bundlePrice.tierFivePrice * (10**decimals())){
            bundlePrice.tierFivePrice = newPrice;
        }else if(bundle == bundlePrice.tierSixPrice * (10**decimals())){
            bundlePrice.tierSixPrice = newPrice;
        }else{
            revert InvalidTier("Invalid Token ID");
        }
    }

    function checkTotalBalanceOf(address user) external view returns (uint256) {

        uint256 totalNFTBalance;

        for (uint256 i = 1; i <= 6; i++) {
            totalNFTBalance += balanceOf(user, i);
        }

        return totalNFTBalance;
    }

    function emergencyWithdrawBalance() external onlyOwner {
        uint256 balance = paymentToken.balanceOf(address(this));
        paymentToken.safeTransfer(msg.sender, balance);
    }
