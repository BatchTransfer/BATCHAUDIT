    }

    /// @dev returns the balances of an address (holder) for specific nft ids
    /// @param _holder address of the holder
    /// @param _nft_ids the ids for which the balance check is being done
    function balanceOfHolder(
        address _holder,
        uint256[] memory _nft_ids
    ) public view virtual returns (uint256[] memory) {
        uint256[] memory batchBalances = new uint256[](_nft_ids.length);

        for (uint256 i = 0; i < _nft_ids.length; ++i) {
            batchBalances[i] = balanceOf(_holder, _nft_ids[i]);
