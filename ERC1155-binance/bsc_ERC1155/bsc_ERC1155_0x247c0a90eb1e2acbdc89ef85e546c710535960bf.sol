        details[0] = mint_stage_data[types.MintStage(_mint_stage)]
            .chest_details[types.ChestType.BRONZE];
        details[1] = mint_stage_data[types.MintStage(_mint_stage)]
            .chest_details[types.ChestType.SILVER];
        details[2] = mint_stage_data[types.MintStage(_mint_stage)]
            .chest_details[types.ChestType.GOLD];
        details[3] = mint_stage_data[types.MintStage(_mint_stage)]
            .chest_details[types.ChestType.PLATINUM];

