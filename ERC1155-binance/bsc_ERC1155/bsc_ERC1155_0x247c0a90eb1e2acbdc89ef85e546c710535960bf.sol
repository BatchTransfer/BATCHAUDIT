    /// @dev utility function which is checking if all the constratins are met in order to mint tokens
    /// either via public or private mint
    /// @param _chest_types array containing the types of chest which are going to be minted
    /// @param _amounts array containing the amount which is going to ve minted for each chest_type
    function _checkMintConstraints(
        uint8[] memory _chest_types,
        uint16[] memory _amounts
    ) internal virtual {
        // check if there is a mismatch between parameters length
        if (_chest_types.length != _amounts.length)
            revert ArraysLengthMismatch();

        // check if all the provided chest types are correct
        for (uint8 i = 0; i < _chest_types.length; i++) {
            if (
                _chest_types[i] < uint8(types.ChestType.BRONZE) ||
                _chest_types[i] > uint8(types.ChestType.PLATINUM)
            ) revert InvalidChestType();
        }
    }

    /// @dev utility function which is checking if all the constratins are met in order to mint tokens
    /// by calling the public mint function
    /// @param _chest_types array containing the types of chest which are going to be minted
    /// @param _amounts array containing the amount which is going to ve minted for each chest_type
    function _checPublickMintConstraints(
        uint8[] memory _chest_types,
        uint16[] memory _amounts
    ) internal virtual {
        // check if the user is whitelisted for the current mint stage
        if (
            mint_stage_whitelist[current_mint_stage][msg.sender] !=
            mint_stage_data[current_mint_stage].stage.requires_whitelist
        ) revert NotWhitelisted(current_mint_stage, msg.sender);

        // total number of chests being minted
        uint16 total_mint_amount = 0;

        // total price value
        uint256 total_price_value = 0;

        // number of chests being minted for each chest type
        uint16[] memory amount_by_chest_type = new uint16[](
            types.CHEST_TYPES_LENGTH
        );

        // compute the above values
        for (uint8 i = 0; i < _chest_types.length; i++) {
            total_mint_amount += _amounts[i];
            total_price_value +=
                mint_stage_data[current_mint_stage]
                    .chest_details[types.ChestType(_chest_types[i])]
                    .price *
                _amounts[i];
            amount_by_chest_type[
                uint8(types.ChestType(_chest_types[i]))
            ] += _amounts[i];
        }

        // check if the amounts minted not exceed the chests supplies
        for (uint8 i = 0; i < types.CHEST_TYPES_LENGTH; i++) {
            if (
                amount_by_chest_type[i] +
                    mint_stage_data[current_mint_stage]
                        .chest_details[types.ChestType(i)]
                        .minted >
                mint_stage_data[current_mint_stage]
                    .chest_details[types.ChestType(i)]
                    .max_supply
            ) revert ChestSupplyLimitExceeded(types.ChestType(i));
        }

        // check if the total amount minted not exceeds the limit of chests per user for the current stage
        if (
            holder_data[current_mint_stage][msg.sender].total_supply +
                total_mint_amount >
            mint_stage_data[current_mint_stage].stage.max_chests
        ) revert UserMintLimitExceeded();

        // check if the total amount minted meets the limit of chests per user for the current stage
        if (
            holder_data[current_mint_stage][msg.sender].total_supply +
                total_mint_amount <
            mint_stage_data[current_mint_stage].stage.min_chests
        ) revert UserMintLimitNotReached();

        // check if the bnb value sent equals the total value of the chests to be minted
        if (total_price_value != msg.value) revert FundsMismatch();

        // check if the total mint amount together with already minted chests don't exceed the stage limit
        if (
            mint_stage_data[types.MintStage(current_mint_stage)]
                .stage
                .minted_chests +
                total_mint_amount >
            mint_stage_data[types.MintStage(current_mint_stage)]
                .stage
                .total_chests
        ) revert StageSupplyLimitExceeded(types.MintStage(current_mint_stage));
    }

    /// @dev utility function which is checking if all the constratins are met in order to mint tokens
    /// by calling the private mint function
    /// @param _chest_types array containing the types of chest which are going to be minted
    /// @param _amounts array containing the amount which is going to ve minted for each chest_type
    function _checkPrivatekMintConstraints(
        uint8 _mint_stage,
        uint8[] memory _chest_types,
        uint16[] memory _amounts
    ) internal virtual {
        // check if the mint stage is correct
        if (
            _mint_stage != uint8(types.MintStage.TEAM_AND_PARTNERS) &&
            _mint_stage != uint8(types.MintStage.IN_GAME_REWARDS)
        ) revert WrongMintStage();

        // total number of chests being minted
        uint16 total_mint_amount = 0;

        // number of chests being minted for each chest type
        uint16[] memory amount_by_chest_type = new uint16[](
            types.CHEST_TYPES_LENGTH
        );

        // compute the above values
        for (uint8 i = 0; i < _chest_types.length; i++) {
            total_mint_amount += _amounts[i];
            amount_by_chest_type[
                uint8(types.ChestType(_chest_types[i]))
            ] += _amounts[i];
        }

        // check if the amounts minted not exceed the chests supplies
        for (uint8 i = 0; i < types.CHEST_TYPES_LENGTH; i++) {
            if (
                amount_by_chest_type[i] +
                    mint_stage_data[types.MintStage(_mint_stage)]
                        .chest_details[types.ChestType(i)]
                        .minted >
                mint_stage_data[types.MintStage(_mint_stage)]
                    .chest_details[types.ChestType(i)]
                    .max_supply
            ) revert ChestSupplyLimitExceeded(types.ChestType(i));
        }
        // check if the total mint amount together with already minted chests don't exceed the stage limit
        if (
            mint_stage_data[types.MintStage(_mint_stage)].stage.minted_chests +
                total_mint_amount >
            mint_stage_data[types.MintStage(_mint_stage)].stage.total_chests
        ) revert StageSupplyLimitExceeded(types.MintStage(_mint_stage));
    }

    /// @param _chest_types array containing the types of chest which are going to be minted
    /// @param _amounts array containing the amount which is going to ve minted for each chest_type
    function _updateRelevantData(
        uint8 _mint_stage,
        uint8[] memory _chest_types,
        uint16[] memory _amounts
    ) internal {
        for (uint8 i = 0; i < _chest_types.length; i++) {
            mint_stage_data[types.MintStage(_mint_stage)]
                .stage
                .minted_chests += _amounts[i];
            mint_stage_data[types.MintStage(_mint_stage)]
                .chest_details[types.ChestType(_chest_types[i])]
                .minted += _amounts[i];
            holder_data[types.MintStage(_mint_stage)][msg.sender].chests_supply[
                    types.ChestType(_chest_types[i])
                ] += _amounts[i];
            holder_data[types.MintStage(_mint_stage)][msg.sender]
                .total_supply += _amounts[i];
        }
    }

    /// @dev converts an array of chest types to an array of nft ids based of the current mint stage
    /// @param _chest_types array containing the types of chest which are going to be minted
    function _chestTypesToTokenId(
        uint8[] memory _chest_types
    ) internal virtual returns (uint256[] memory) {
        uint256[] memory token_ids = new uint256[](_chest_types.length);
        for (uint8 i = 0; i < _chest_types.length; i++) {
            token_ids[i] = uint256(
                uint8(current_mint_stage) *
                    types.CHEST_TYPES_LENGTH +
                    _chest_types[i]
            );
        }
        return token_ids;
    }

    /// @dev converts an array of uint16 values to an array of uin256 values
    /// @param _uint16Array array of uint16 values which is going to be converted
    function _convertUint16ToUint256(
        uint16[] memory _uint16Array
    ) public pure returns (uint256[] memory) {
        uint256 length = _uint16Array.length;
        uint256[] memory uint256Array = new uint256[](length);

        assembly {
            // Load the starting memory locations of both arrays
            let uint16ArrayPtr := add(_uint16Array, 0x20)
            let uint256ArrayPtr := add(uint256Array, 0x20)

            // Iterate over the array
            for {
                let i := 0
            } lt(i, length) {
                i := add(i, 1)
            } {
                // Load the uint16 value, zero-extend it to 256 bits, and store it in the new array
                let uint16Value := mload(add(uint16ArrayPtr, mul(i, 0x20)))
                mstore(add(uint256ArrayPtr, mul(i, 0x20)), uint16Value)
            }
        }

        return uint256Array;
    }
}


