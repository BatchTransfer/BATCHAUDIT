    function setIsWhitelisted(
        address _address,
        uint8 _mint_stage,
        bool _is_whitelisted
    ) public onlyRole(DEFAULT_ADMIN_ROLE) nonReentrant {
        if (
            mint_stage_data[types.MintStage(_mint_stage)]
                .stage
                .requires_whitelist == false
        ) revert PublicMintStage(types.MintStage(_mint_stage));

        mint_stage_whitelist[types.MintStage(_mint_stage)][
            _address
        ] = _is_whitelisted;

        emit WhitelistUpdate(
            types.MintStage(_mint_stage),
            _address,
            _is_whitelisted
        );
    }

    // ===================== MINT STAGE FUNCTIONS =====================

    /// @dev used to set the next mint stage if there are any left
    function moveToNextAllocationStage() public onlyRole(DEFAULT_ADMIN_ROLE) {
        if (current_mint_stage == types.MintStage.IN_GAME_REWARDS) {
            revert LastMintStageReached();
        }

        current_mint_stage = types.MintStage(uint8(current_mint_stage) + 1);

        emit MintStageStarted(current_mint_stage);
    }

    // ===================== MINT FUNCTIONS =====================

    /// @dev used to mint tokens from the current allocation stage. Can be called by anyone
    /// @param _to address which is going to receive the minted nfts
    /// @param _chest_types array containing the types of chest which are going to be minted
    /// @param _amounts array containing the amount which is going to ve minted for each chest_type
    function publicMint(
        address _to,
        uint8[] memory _chest_types,
        uint16[] memory _amounts,
        bytes memory _data
    ) public payable nonReentrant {
        _checkMintConstraints(_chest_types, _amounts);
        _checPublickMintConstraints(_chest_types, _amounts);

        _updateRelevantData(uint8(current_mint_stage), _chest_types, _amounts);

        uint256[] memory nft_ids = _chestTypesToTokenId(_chest_types);
        uint256[] memory nft_amounts = _convertUint16ToUint256(_amounts);

        _mintBatch(_to, nft_ids, nft_amounts, _data);
    }

    /// @dev used to mint tokens from the either team or rewards stages. Can be called by anyone
    /// @param _to address which is going to receive the minted nfts
    /// @param _chest_types array containing the types of chest which are going to be minted
    /// @param _amounts array containing the amount which is going to ve minted for each chest_type
    function privateMint(
        uint8 _mint_stage,
        address _to,
        uint8[] memory _chest_types,
        uint16[] memory _amounts,
        bytes memory _data
    ) public payable nonReentrant onlyRole(DEFAULT_ADMIN_ROLE) {
        _checkMintConstraints(_chest_types, _amounts);
        _checkPrivatekMintConstraints(_mint_stage, _chest_types, _amounts);

        _updateRelevantData(_mint_stage, _chest_types, _amounts);

        uint256[] memory nft_ids = _chestTypesToTokenId(_chest_types);
        uint256[] memory nft_amounts = _convertUint16ToUint256(_amounts);

        _mintBatch(_to, nft_ids, nft_amounts, _data);
    }

