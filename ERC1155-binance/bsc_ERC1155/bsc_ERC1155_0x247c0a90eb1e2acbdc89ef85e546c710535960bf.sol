 * @notice This library provides data structures used for managing chest types, sale stages, and holder balances
 * in a chest minting system.
 */
library DataTypes {
    // ===================== CONSTANTS =====================

    /// @dev Total number of available chest types.
    uint8 constant CHEST_TYPES_LENGTH = 4;

    uint8 constant MINT_STAGES_LENGTH = 7;

    // ===================== ENUMS =====================

    /// @dev Enumerates all possible chest types.
    /// @notice The different rarities of chests available.
    enum ChestType {
        BRONZE,
        SILVER,
        GOLD,
        PLATINUM
    }

    /// @dev Enumerates all possible mint periods for the chests.
    /// @notice The different sale stages in which users can mint chests.
    enum MintStage {
        PRIVATE_SALE,
        WHITELIST,
        PUBLICSALE_ONE,
        PUBLICSALE_TWO,
        PUBLICSALE_THREE,
        TEAM_AND_PARTNERS,
        IN_GAME_REWARDS
    }

    // ===================== STRUCTS =====================

    /// @dev Defines minting conditions for a specific sale stage.
    struct MintStageDetails {
        uint8 min_chests; // Minimum number of mintable chests in this stage.
        uint16 max_chests; // Maximum number of mintable chests in this stage.
        uint16 total_chests; // Number of chests in this stage
        uint16 minted_chests; // Number of chests minted in this stage
        bool requires_whitelist; // Whether whitelisting is required for this stage.
    }

    /// @dev Defines price and max supply for a chest type in a specific sale stage.
    struct MintStageChestTypeDetails {
        uint64 price; // Price of a chest of this type in this stage.
        uint16 max_supply; // Maximum supply of this chest type in this stage.
        uint16 minted; // Number of chests minted so far
    }

    /// @dev Combines information for a specific mint stage, including stage data and chest type data.
    struct MintStageData {
        MintStageDetails stage; // Data related to the mint stage itself (min/max chests, whitelist requirement)
        mapping(ChestType => MintStageChestTypeDetails) chest_details; // Array of chest data for each chest type in this stage.
    }

