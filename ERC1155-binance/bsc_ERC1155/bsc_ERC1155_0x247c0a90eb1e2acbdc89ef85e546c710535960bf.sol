    /// @dev used to unlock the transfer of the nfts
    function unlockTransfers() public onlyRole(DEFAULT_ADMIN_ROLE) {
        transfers_allowed = true;
    }

    // ===================== WHITDRAW FUNCTION =====================

    /// @dev used to whitdraw funds from the contract
