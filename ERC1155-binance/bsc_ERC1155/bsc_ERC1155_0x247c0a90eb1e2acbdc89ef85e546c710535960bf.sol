pragma solidity ^0.8.24;
import {DataTypes as types} from "../libraries/DataTypes.sol";

/**
 * @title Realms Of Glory Chests Contract Interface
 * @author Realms of Glory
 * @notice Interface for the Realms Of Glory Chests contract
 */

interface IRealmsOfGloryChests {
    // ===================== ERRORS =====================

    /// @notice Triggered when trying to whitelist an address for a public mint stage
    error PublicMintStage(types.MintStage mint_stage);

    /// @notice Triggered when the last allocation stage has been reached but a call to move to next one is being done
    error LastMintStageReached();

    /// @notice Triggered when the length between chest types and amounts arrays don't have the same lenght
    error ArraysLengthMismatch();

    /// @notice Triggered when the user is not whitlisted for the mint stage during which he is trying to mint
    error NotWhitelisted(types.MintStage mint_stage, address user_address);

    /// @notice Thrown when the provided chest type is not correct
    error InvalidChestType();

    /// @notice Thrown when the amount of chests of one type which is being minted, exceeds the maximum supply for that specific type
    error ChestSupplyLimitExceeded(types.ChestType chest_type);

    /// @notice Thrown when the amount of chests being minted exceeds the maximum supply for that specific stage
    error StageSupplyLimitExceeded(types.MintStage mint_stage);

    /// @notice Thrown when the amount of chests being minted together with the balance of user are exceeding the mint limit per user
    error UserMintLimitExceeded();

    /// @notice Thrown when the amount of chests being minted together with the balance of user are exceeding don't meet the mint limit per user
    error UserMintLimitNotReached();

    /// @notice Thrown if the bnb value of the chests minted doesn't equals witht the bnb value sent
    error FundsMismatch();

    /// @notice Thrown if the private mint is being called for wrong mint stage
    error WrongMintStage();

    // ===================== EVENTS =====================

    /// @notice Triggered when the whitelist status for an addess is being updated for a mint stage
    event WhitelistUpdate(
        types.MintStage mint_stage,
        address user_address,
        bool is_whitelisted
    );

    /// @notice Triggered when a new mint stage starts
    event MintStageStarted(types.MintStage mint_stage);
}


