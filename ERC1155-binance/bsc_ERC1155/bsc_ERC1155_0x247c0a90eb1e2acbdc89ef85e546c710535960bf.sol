        returns (
            uint16 total_supply,
            uint16 bronze_supply,
            uint16 silver_supply,
            uint16 gold_supply,
            uint16 plantinum_supply
        )
    {
        return (
            holder_data[types.MintStage(_mint_stage)][_holder].total_supply,
            holder_data[types.MintStage(_mint_stage)][_holder].chests_supply[
                types.ChestType.BRONZE
