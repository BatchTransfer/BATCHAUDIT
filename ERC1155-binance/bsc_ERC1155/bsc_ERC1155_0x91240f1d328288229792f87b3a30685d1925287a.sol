import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/cryptography/EIP712.sol";


import "./interfaces/INFTTemplate.sol";
import {MintRequest, MINT_REQUEST_TYPE_HASH} from "./libraries/MintRequestLib.sol";

/**
 * @title NFT Template
 * @dev This will be the blueprint for each individual NFT contract that
 *      the factory creates. It contains the standard ERC-1155 methods
 *      and additional functions specific to the NFTs.
 */
contract NFTTemplate is ERC1155, Ownable, ERC1155Pausable, ERC1155Burnable, ERC1155Supply, INFTTemplate, ReentrancyGuard, EIP712 {
    uint256 public constant MINT_AMOUNT = 1;

    using ECDSA for bytes32;
    uint256 public maxSupply;

    // a variable to keep track of NFT ids
    uint256 public id;

    // Dynamic state variables for domain and version
    string private _signingDomain;
    string private _signatureVersion;

    // Mapping to track which addresses have already minted
    mapping(address => bool) public hasMinted;
    mapping(address => bool) public isAddressAllowedToMint;

    constructor(
        address initialOwner, 
        string memory _uri, 
        uint256 _maxSupply,
        string memory signingDomain, 
        string memory signatureVersion) 
        ERC1155(_uri) 
        Ownable(initialOwner) 
        EIP712(signingDomain, signatureVersion) {
        maxSupply = _maxSupply;
        _signingDomain = signingDomain;
        _signatureVersion = signatureVersion;
    }


    // allows to update URI
    function setURI(string memory newuri) public onlyOwner {
        // set new uri
        _setURI(newuri);
        emit URIUpdated(newuri);
    }

    // Function to access the current signing domain and version
    function getSigningDetails() public view returns (string memory domain, string memory version) {
        return (_signingDomain, _signatureVersion);
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    // a function to generate a new NFT id
    function generateId() internal returns (uint256) {
        // increment id
        id++;
        return id;
    }

    function addToWhitelist(address _address) public onlyOwner {
        require(!isAddressAllowedToMint[_address], "Address already whitelisted");
        // add address to whitelist
        isAddressAllowedToMint[_address] = true;
        emit AddressWhitelisted(_address);
    }


    // when a user deletes their account with viewfi, we need to remove them from the whitelist
    function removeFromWhitelist(address _address) public onlyOwner {
        require(isAddressAllowedToMint[_address], "Address not in whitelist");
        // remove address from whitelist
        isAddressAllowedToMint[_address] = false;
        emit AddressRemovedFromWhitelist(_address);
    }

    function verifyMintIsAllowed(address _address) internal view {
        // Check that receiver is whitelisted
        require(isAddressAllowedToMint[_address], "Address is not allowed to mint");
        // Check that receiver has not already minted
        require(!hasMinted[_address], "Address has already minted");
    }

    function _mintFrom(address receiver, 
                       bytes memory data,
                       bool gasless) internal {
        // create id
        uint256 nft_id = generateId();
        // check if total supply is less than max supply
        require(totalSupply(nft_id) + MINT_AMOUNT <= maxSupply, "Exceeds max supply");
        // mint the NFT
        _mint(receiver, nft_id, MINT_AMOUNT, data);
        // set hasMinted to true
        hasMinted[receiver] = true;
        // emit event
        emit Minted(receiver, nft_id, MINT_AMOUNT, data, gasless);
    }

    function mint(bytes memory data) public nonReentrant whenNotPaused {
        verifyMintIsAllowed(_msgSender());
        _mintFrom(msg.sender, data, false);
    }

    function domainSeparator() public view returns (bytes32) {
        return _domainSeparatorV4();
    }

    /**
    * @notice sponsor can mint on behalf of a user
    * @dev Allows for gasless minting of NFTs through EIP-712 signature verification. Users sign a minting request off-chain,
    * which is then submitted on-chain by another party who pays the gas fees. This function verifies the authenticity of
    * the signed message, ensuring it comes from an authorized user and adheres to the contract's minting requirements.
    * It leverages the security and user-friendly features of EIP-712 typed data signing to provide a secure and efficient
    * minting process. The function enforces minting permissions, reentrancy protection, and contract pause state checks
    * to maintain integrity and control over the minting operation.
    *
    * @param request The minting request containing the details of the NFT to be minted, structured according to the
    * MintRequest struct. This includes the recipient's address, the amount of the NFT to mint, a unique nonce for the request,
    * and any additional data required for minting.
    * @param signature The signature generated off-chain by the user, signing the minting request. This signature is verified
    * on-chain to authenticate the request and authorize the minting operation.
    */
    function gaslessMint(MintRequest calldata request, bytes calldata signature) public nonReentrant whenNotPaused {
        verifyMintIsAllowed(request.to);

        // Verify the signature
        // Create a digest of the signed data using EIP-712 typed data signing standards.
        // This allows for more secure and understandable off-chain signatures.
        bytes32 digest = _hashTypedDataV4(
            keccak256(
                abi.encode(
                    MINT_REQUEST_TYPE_HASH,
                    request.to,
                    keccak256(bytes(request.data))
                )
            )
        );

        // Recover the signer's address from the signature and the digest to ensure the signature is valid
        // and was indeed signed by the address making the request.
        address signer = digest.recover(signature);

        // Check that the recovered signer is the same as the 'to' address in the request to prevent unauthorized actions.
        require(signer == request.to, "Invalid signature");

        // Perform the mint
        _mintFrom(request.to, request.data, true);
    }

    // The following functions are overrides required by Solidity.
    function _update(address from, address to, uint256[] memory ids, uint256[] memory values)
        internal
        override(ERC1155, ERC1155Pausable, ERC1155Supply)
    {
        super._update(from, to, ids, values);
    }
}


