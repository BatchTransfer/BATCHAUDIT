pragma solidity ^0.8.20;

import "@openzeppelin/contracts/access/Ownable.sol";

import "./NFTTemplate.sol";
import "./interfaces/INFTFactory.sol";

/**
 * @title NFT Factory
 * @dev This contract will be responsible for deploying other NFT contracts.
 *      Each time you want to create a new NFT series or collection,
 *      the factory will deploy a new NFT contract instance.
 */
contract NFTFactory is INFTFactory, Ownable {
    address[] public deployedNFTCollections; // To keep track of all deployed NFTTemplates
    mapping(address => address[]) public userToNFTCollections;

    // Add this constructor
    constructor(address _initialOwner) Ownable(_initialOwner) {}

    /**
     * @notice Deploy a new NFT collection.
     * @param _uri The base URI for the new NFT collection.
     * @param _maxSupply The maximum supply for this collection.
     * @return The address of the newly created NFT collection.
     * @dev Only the owner of the factory can call this function.
     *  signingDomain and signatureVersion are used as part of the EIP-712 standard to create a unique domain
     *  for signing messages. This helps in preventing signature replay attacks across different contracts and domains.
    */
    function createNFTCollection(string memory _uri, uint256 _maxSupply, string memory signingDomain, string memory signatureVersion) public onlyOwner returns (address) {
        require(_maxSupply > 0, "NFTFactory: Max supply must be greater than 0");

        NFTTemplate newNFTContract = new NFTTemplate(owner(), _uri, _maxSupply, signingDomain, signatureVersion); // add metadata.json URL

        deployedNFTCollections.push(address(newNFTContract)); // Store the deployed address

        // Add the contract address to the creator's array in the mapping
        userToNFTCollections[msg.sender].push(address(newNFTContract));

        emit NFTCollectionCreated(msg.sender, address(newNFTContract), _uri, _maxSupply);

        return address(newNFTContract);
    }

    // To get the count of deployed NFTTemplates
    function getDeployedNFTCollectionsCount() public view returns (uint256) {
        return deployedNFTCollections.length;
    }

    // To get the array of deployed NFTTemplates for a specific user
    function getNFTCollectionsForUser(address user) public view returns (address[] memory) {
        return userToNFTCollections[user];
    }
}


