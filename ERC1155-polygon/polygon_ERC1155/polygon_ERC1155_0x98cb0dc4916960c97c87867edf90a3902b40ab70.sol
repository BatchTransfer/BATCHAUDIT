pragma solidity =0.8.24;












contract NAP0 is Ownable(msg.sender), INAP1155, ERC1155Holder, ReentrancyGuard {
    using SafeERC20 for IERC20;
    using SafeCast for uint256;

    uint64 private constant ONE_DAY_IN_BLOCKS = 39_273;
    uint16 private constant DENOMINATOR = 10_000;
    uint256 public b01Threshold = 100 * (10 ** 18);
    address public nft;
    address public treasury;
    uint256[] public allTokenIds;
    mapping(uint256 => uint256) public percentage;

    IERC20 public assuranceToken;
    IERC20 public collateralToken;
    IERC20 public b01Token;
    IUniswapV2Pair public lpAssurance;
    IUniswapV2Router02 public uniswapRouter;
    address public feeRecipient;

    uint16 public treasuryFeePercentage;
    uint16 public deactivationFeePercentage;

    uint64 public intervalDuration;
    uint64 public intervalDurationUpdatedAt;
    uint64 public intervalDurationUpdatedAtIndex;
    uint16 public maxDeactivationsPerInterval;

    mapping(uint64 => uint16) public deactivationsCountByInterval;

    struct ActivationData {
        uint256 amountInA;
        uint256 amountInB;
        address[] path;
        uint256 totalAmountInA;
    }

    event UpdatePercentages(uint256[] tokenIds, uint256[] percentages);

    /// @param _assuranceToken The address of the assurance token contract
    /// @param _feeRecipient The address where fees will be sent
    /// @param _nft The address of the NFT
    /// @param _collateralToken The address of the collateral token contract
    /// @param _b01Token Additional ERC20 token for contract logic
    /// @param _uniswapRouter The address of the Uniswap Router contract
    /// @param _treasury Address of the treasury for receiving a portion of the fees
    /// @param _deactivationFeePercentage The percentage of the token price to charge when deactivating tokens
    /// @param _maxDeactivationsPerInterval The maximum number of tokens that can be deactivated in a single interval
    constructor(
        IERC20 _assuranceToken,
        address _feeRecipient,
        address _nft,
        IERC20 _collateralToken,
        IERC20 _b01Token,
        IUniswapV2Router02 _uniswapRouter,
        address _treasury,
        uint16 _treasuryFeePercentage,
        uint16 _deactivationFeePercentage,
        uint16 _maxDeactivationsPerInterval
    ) {
        require(address(_assuranceToken) != address(0), "Invalid Assurance Token");
        require(_feeRecipient != address(0), "Invalid Fee Recipient");
        require(_nft != address(0), "Invalid NFT Address");
        require(address(_collateralToken) != address(0), "Invalid Collateral Token");
        require(address(_b01Token) != address(0), "Invalid b01 Token");
        require(address(_uniswapRouter) != address(0), "Invalid Uniswap Router");
        require(_treasury != address(0), "Invalid Treasury");
        // require(_deactivationFeePercentage > 0, "Deactivation Fee must be > 0"); Currently under discussion
        require(_deactivationFeePercentage <= 1000, "Deactivation Fee must be <= 10%");
        require(_maxDeactivationsPerInterval > 0, "Max Deactivations must be > 0");
        nft = _nft;
        assuranceToken = _assuranceToken;
        collateralToken = _collateralToken;
        uniswapRouter = _uniswapRouter;
        b01Token = _b01Token;

        IUniswapV2Factory uniswapFactory = IUniswapV2Factory(_uniswapRouter.factory());
        IUniswapV2Pair lpAssurance_ = IUniswapV2Pair(
            uniswapFactory.getPair(address(_assuranceToken), address(_collateralToken))
        );
        lpAssurance = lpAssurance_;
        feeRecipient = _feeRecipient;
        treasury = _treasury;

        setTreasuryFeePercentage(_treasuryFeePercentage);
        setDeactivationFeePercentage(_deactivationFeePercentage);
        setMaxDeactivationsPerInterval(_maxDeactivationsPerInterval);
        setIntervalDuration(ONE_DAY_IN_BLOCKS); /// ≈1 day in blocks

        _assuranceToken.approve(address(uniswapRouter), type(uint256).max);
        _collateralToken.approve(address(uniswapRouter), type(uint256).max);
        IERC20(address(lpAssurance_)).approve(address(uniswapRouter), type(uint256).max);
    }

    /// @notice Calculates the activation price for given tokens.
    /// @param tokenIn The token used for activation.
    /// @param tokenIds IDs of the tokens to activate.
    /// @param amounts Amounts of each token to activate.
    /// @return amountIn Total activation price.
    function getActivationPrice1155(
        address tokenIn,
        uint256[] calldata tokenIds,
        uint256[] calldata amounts
    ) external view returns (uint256 amountIn) {
        amountIn = _getActivationData(tokenIn, tokenIds, amounts).totalAmountInA;
    }

    /// @notice Retrieves the available count for deactivation in the current interval.
    /// @return Available deactivation count for the current interval.
    function getAvailableDeactivationCount() external view returns (uint16) {
        uint16 _maxDeactivationsPerInterval = maxDeactivationsPerInterval;
        uint64 currentInterval = getCurrentInterval();
        uint16 currentDeactivationsCount = deactivationsCountByInterval[currentInterval];
        if (currentDeactivationsCount >= _maxDeactivationsPerInterval) {
            return 0;
        }
        return _maxDeactivationsPerInterval - currentDeactivationsCount;
    }

    /// @notice Removes percentage allocation for specified token IDs.
    /// @param tokenIds The IDs of the tokens to remove percentages for.
    function removeTokenPercentages(uint256[] calldata tokenIds) external onlyOwner {
        for (uint256 i; i < tokenIds.length; i++) {
            require(percentage[tokenIds[i]] != 0, "ERR: ID does not exist");
            delete percentage[tokenIds[i]];

            for (uint256 j; j < allTokenIds.length; j++) {
                if (allTokenIds[j] == tokenIds[i]) {
                    allTokenIds[j] = allTokenIds[allTokenIds.length - 1];
                    allTokenIds.pop();
                    break;
                }
            }
            require(_checkTotalWeight() != 0, "Total weight can't be null");
        }
    }

    /// @notice Activates the specified NFT tokens by adding liquidity to the Uniswap pool
    /// @dev Requires the transfer of tokens to this contract before activation
    /// @param tokenIn Address of the token being added for activation
    /// @param tokenIds Array of NFT token IDs to activate
    /// @param amounts Array of NFT token amounts to activate
    /// @param amountInMax Maximum amount of `tokenIn` that can be used for activation
    /// @param to Address to receive the activated NFT tokens
    /// @param uniswapDeadline Deadline for the Uniswap transaction
    /// @return totalActivationPrice Total amount of `tokenIn` used for activation
    function activateTokens1155(
        address tokenIn,
        uint256[] calldata tokenIds,
        uint256[] calldata amounts,
        uint256 amountInMax,
        address to,
        uint256 uniswapDeadline
    ) external nonReentrant returns (uint256 totalActivationPrice) {
        require(tokenIds.length == amounts.length, "Mismatched lengths");
        require(tokenIds.length != 0, "Invalid tokenIds");
        require(amountInMax != 0, "Invalid amounts");
        require(to != address(0), "Invalid address to");

        uint256 totalLiquidityAmount;

        for (uint256 i; i < tokenIds.length; i++) {
            uint256 tokenId = tokenIds[i];
            uint256 amount = amounts[i];
            uint256 price = getNFTPrice(tokenId) * amount;
            totalLiquidityAmount += price;
        }

        require(totalLiquidityAmount != 0, "INVALID LIQUIDITY");
        ActivationData memory data = _getActivationData(tokenIn, tokenIds, amounts);

        if (data.totalAmountInA > amountInMax) {
            revert PriceExceedsAmountInMax(data.totalAmountInA, amountInMax);
        }
        IERC20(tokenIn).safeTransferFrom(msg.sender, address(this), data.totalAmountInA);
        uniswapRouter.swapTokensForExactTokens(
            data.amountInB,
            data.totalAmountInA,
            data.path,
            address(this),
            uniswapDeadline
        );

        /// Move liquidity variable declaration inside this block to limit its scope
        uint256 liquidity = _addLiquidity(
            data.amountInA,
            data.amountInB,
            data.path,
            uniswapDeadline
        );
        if (liquidity < (totalLiquidityAmount * 997) / 1000) {
            revert InsufficientLiquidityAdded(liquidity, totalLiquidityAmount);
        }
        totalActivationPrice = data.totalAmountInA;
        ERC1155Supply(nft).safeBatchTransferFrom(address(this), to, tokenIds, amounts, bytes(""));

        emit ActivateTokens(tokenIds, totalActivationPrice, tokenIn);
    }

    /// @notice Sets token percentages for specific token IDs.
    /// @param tokenIds The IDs of the tokens to set percentages for.
    /// @param newWeights The new percentage weights for the tokens.
    function setTokenPercentage(
        uint256[] calldata tokenIds,
        uint256[] calldata newWeights
    ) external onlyOwner {
        require(tokenIds.length == newWeights.length, "Mismatched lengths");
        for (uint256 i; i < tokenIds.length; i++) {
            if (percentage[tokenIds[i]] == 0) {
                allTokenIds.push(tokenIds[i]);
            }

            percentage[tokenIds[i]] = newWeights[i];
        }

        require(_checkTotalWeight() == 10000, "ERR: Incorrect weight values");
        emit UpdatePercentages(tokenIds, newWeights);
    }

    /// @notice Deactivates the specified NFT tokens by removing liquidity from the Uniswap pool and returning the tokens
    /// @param tokenOut Address of the token to receive after deactivation
    /// @param tokenIds Array of NFT token IDs to deactivate
    /// @param amounts Array of NFT token amounts to deactivate
    /// @param deactivationProfitAmountMin Minimum amount of `tokenOut` expected to receive after deactivation
    /// @param to Address to receive the `tokenOut` after deactivation
    /// @return deactivationAmount Actual amount of `tokenOut` received after deactivation
    function deactivateTokens1155(
        address tokenOut,
        uint256[] calldata tokenIds,
        uint256[] calldata amounts,
        uint256 deactivationProfitAmountMin,
        address to,
        uint256 uniswapDeadline
    ) external nonReentrant returns (uint256 deactivationAmount) {
        require(to != address(0), "Invalid address to");
        require(tokenIds.length == amounts.length, "Mismatched lengths");

        uint64 currentInterval = getCurrentInterval();
        uint16 newDeactivationsCount = deactivationsCountByInterval[currentInterval] +
            (tokenIds.length).toUint16();

        if (newDeactivationsCount > maxDeactivationsPerInterval) {
            revert MaxDeactivationAmountPerIntervalExceeded(
                currentInterval,
                maxDeactivationsPerInterval,
                newDeactivationsCount
            );
        }
        deactivationsCountByInterval[currentInterval] = newDeactivationsCount;

        ERC1155Supply(nft).safeBatchTransferFrom(
            msg.sender,
            address(this),
            tokenIds,
            amounts,
            bytes("")
        );

        uint256 deactivationProfitAmount = _removeLiquidityAndSwap(
            calculateTotalLiquidityAmount(tokenIds, amounts),
            tokenOut,
            uniswapDeadline
        );

        deactivationAmount = _processDeactivation(deactivationProfitAmount, tokenOut, to);
        emit DeactivateTokens(tokenIds, deactivationAmount, tokenOut);

        if (deactivationAmount < (deactivationProfitAmountMin * 997) / 1000) {
            revert DeactivationProfitAmountLessThanMin(
                deactivationProfitAmount,
                deactivationProfitAmountMin
            );
        }
    }

    function getDeactivationProfit1155(
        address caller,
        address tokenOut,
        uint256[] calldata tokenIds,
        uint256[] calldata amounts
    ) public view returns (uint256 amountOut) {
        uint256 totalLiquidityAmount = 0;

        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint256 proportion = getNFTPrice(tokenIds[i]) * amounts[i];
            totalLiquidityAmount += proportion;
        }
        if (totalLiquidityAmount == 0) {
            return 0;
        }

        amountOut = calculateAmountOut(totalLiquidityAmount, tokenOut);

        uint256 totalFeePercentage = deactivationFeePercentage +
            (b01Token.balanceOf(caller) >= b01Threshold ? 0 : treasuryFeePercentage);
        uint256 deactivationFee = (amountOut * totalFeePercentage) / DENOMINATOR;
        uint256 deactivationAmount = amountOut - deactivationFee;
        amountOut = deactivationAmount;
        return amountOut;
    }

    function calculateAmountOut(
        uint256 totalLiquidityAmount,
        address tokenOut
    ) internal view returns (uint256 amountOut) {
        (uint256 reserve0, uint256 reserve1) = _calculateTokensForLiquidity();
        uint256 initialAmountOut;
        uint256 amountIn;
        uint256 lpTotalSupply = lpAssurance.balanceOf(address(this));
        uint256 token0Amount = (totalLiquidityAmount * reserve0) / lpTotalSupply;
        uint256 token1Amount = (totalLiquidityAmount * reserve1) / lpTotalSupply;

        address[] memory path = new address[](2);
        if (tokenOut == lpAssurance.token0()) {
            path[0] = lpAssurance.token1();
            path[1] = lpAssurance.token0();
            initialAmountOut = token0Amount;
            amountIn = token1Amount;
        } else if (tokenOut == lpAssurance.token1()) {
            path[0] = lpAssurance.token0();
            path[1] = lpAssurance.token1();
            amountIn = token0Amount;
            initialAmountOut = token1Amount;
        } else {
            revert("Invalid tokenOut");
        }

        uint256[] memory amountsOut = uniswapRouter.getAmountsOut(amountIn, path);
        amountOut = initialAmountOut + amountsOut[1];
    }

    /// @notice Sets the deactivation fee percentage for the NAP721 contract.
    /// @param newDeactivationFeePercentage The new deactivation fee percentage to be set.
    function setDeactivationFeePercentage(uint16 newDeactivationFeePercentage) public onlyOwner {
        require(newDeactivationFeePercentage != 0, "Invalid DeactivationFee");
        if (newDeactivationFeePercentage > 1000) {
            revert InvalidDeactivationFeePercentage(newDeactivationFeePercentage);
        }
        deactivationFeePercentage = newDeactivationFeePercentage;

        emit DeactivationFeePercentageUpdated(newDeactivationFeePercentage);
    }

    /// @notice Calculates the price of a single NFT based on its tokenId by dividing the total weighted amount by the total supply of that tokenId
    /// @param tokenId The ID of the NFT token to get the price for
    /// @return price The calculated price of a single NFT token
    function getNFTPrice(uint256 tokenId) public view returns (uint256 price) {
        require(percentage[tokenId] > 0, "tokenId not active");
        uint256 totalSupply = ERC1155Supply(nft).totalSupply(tokenId);
        require(totalSupply > 0, "Total supply is zero");
        uint256 totalValue = (lpAssurance.balanceOf(address(this)) * percentage[tokenId]) / 10000;
        price = totalValue / totalSupply;
    }

    /// @notice Sets the treasury fee percentage for the NAP721 contract.
    /// @param _treasuryFeePercentage The new treasury fee percentage to be set.
    function setTreasuryFeePercentage(uint16 _treasuryFeePercentage) public onlyOwner {
        require(_treasuryFeePercentage <= 150, "InvalidTreasuryFee");
        treasuryFeePercentage = _treasuryFeePercentage;
    }

    /// @notice Sets the minimum b01 threshold for treasuryFeeLess transfer.
    /// @param newB01Threshold The new threshol.
    /// @dev with keep ability to set arbitary threshold in master contract but it woun't be available from factory
    function setB01Threshold(uint256 newB01Threshold) public onlyOwner {
        b01Threshold = newB01Threshold;
    }

    /// @notice Sets the maximum number of deactivations allowed per interval.
    /// @param newMaxDeactivationsPerInterval The new max deactivations per interval.
    function setMaxDeactivationsPerInterval(uint16 newMaxDeactivationsPerInterval) public onlyOwner {
        maxDeactivationsPerInterval = newMaxDeactivationsPerInterval;

        emit MaxDeactivationsPerIntervalUpdated(newMaxDeactivationsPerInterval);
    }

    /// @notice Updates the duration of each interval.
    /// @param newIntervalDuration The new duration for intervals.
    function setIntervalDuration(uint64 newIntervalDuration) public onlyOwner {
        require(newIntervalDuration != 0, "Invalid Interval");
        uint64 currentBlock = uint64(block.number);
        if (intervalDuration == 0) {
            intervalDurationUpdatedAtIndex = 0;
        } else {
            intervalDurationUpdatedAtIndex = getCurrentInterval() + 1;
        }
        intervalDurationUpdatedAt = currentBlock;
        intervalDuration = newIntervalDuration;

        emit IntervalDurationUpdated(newIntervalDuration, currentBlock);
    }

    /// @notice Determines the current interval based on time elapsed.
    /// @return The index of the current interval.
    function getCurrentInterval() public view returns (uint64) {
        return
            (uint64(block.number) - intervalDurationUpdatedAt) /
            intervalDuration +
            intervalDurationUpdatedAtIndex;
    }

    /// @dev Adds liquidity to the Uniswap pool for the contract's NFT and token pair
    /// @param amountInA Amount of the first token in the pair to add
    /// @param amountInB Amount of the second token in the pair to add
    /// @param path An array of token addresses that the swap will take
    /// @param uniswapDeadline Deadline for the Uniswap transaction
    /// @return liquidity The amount of liquidity tokens received from the pool
    /// @notice This internal function is called during the activation
    function _addLiquidity(
        uint256 amountInA,
        uint256 amountInB,
        address[] memory path,
        uint256 uniswapDeadline
    ) internal returns (uint256 liquidity) {
        (, , liquidity) = uniswapRouter.addLiquidity(
            path[0],
            path[1],
            amountInA,
            amountInB,
            0,
            0,
            address(this),
            uniswapDeadline
        );
        return liquidity;
    }

    /// @dev Removes liquidity from the Uniswap pool and swaps it for a specified token
    /// @param liquidity The amount of liquidity tokens to remove
    /// @param tokenOut The address of the token to swap the liquidity into
    /// @param uniswapDeadline Deadline for the Uniswap transaction
    /// @return amountOut The amount of `tokenOut` tokens received after the swap
    /// @notice This function is used during the deactivation of tokens to remove liquidity from the Uniswap pool and swap it for the desired token
    function _removeLiquidityAndSwap(
        uint256 liquidity,
        address tokenOut,
        uint256 uniswapDeadline
    ) internal returns (uint256 amountOut) {
        (uint256 amountAssurance, uint256 amountCollateral) = uniswapRouter.removeLiquidity(
            address(assuranceToken),
            address(collateralToken),
            liquidity,
            0,
            0,
            address(this),
            uniswapDeadline
        );

        address[] memory path = new address[](2);
        uint256 amountIn;
        uint256 initialAmountOut;

        if (tokenOut == address(assuranceToken)) {
            initialAmountOut = amountAssurance;
            amountIn = amountCollateral;
            path[0] = address(collateralToken);
            path[1] = address(assuranceToken);
        } else if (tokenOut == address(collateralToken)) {
            initialAmountOut = amountCollateral;
            amountIn = amountAssurance;
            path[0] = address(assuranceToken);
            path[1] = address(collateralToken);
        } else {
            revert("Unsupported token address");
        }
        uint256[] memory amounts = uniswapRouter.swapExactTokensForTokens(
            amountIn,
            2,
            path,
            address(this),
            uniswapDeadline
        );

        amountOut = initialAmountOut + amounts[1];

        return amountOut;
    }

    function _calculateTokensForLiquidity()
        internal
        view
        returns (uint256 amount0, uint256 amount1)
    {
        uint256 liquidityBalanceOfNAP = lpAssurance.balanceOf(address(this));
        uint256 totalSupply = lpAssurance.totalSupply();
        (uint112 reserve0, uint112 reserve1, ) = lpAssurance.getReserves();

        amount0 = (reserve0 * liquidityBalanceOfNAP) / totalSupply;
        amount1 = (reserve1 * liquidityBalanceOfNAP) / totalSupply;

        return (amount0, amount1);
    }

    function calculateTotalLiquidityAmount(
        uint256[] calldata tokenIds,
        uint256[] calldata amounts
    ) internal view returns (uint256) {
        uint256 totalLiquidityAmount;
        for (uint256 i; i < tokenIds.length; i++) {
            uint256 amount = amounts[i];
            require(amount != 0, "Malicious amount");
            totalLiquidityAmount += getNFTPrice(tokenIds[i]) * amount;
        }
        require(totalLiquidityAmount != 0, "Invalid liquidity");
        return totalLiquidityAmount;
    }

    function _processDeactivation(
        uint256 _deactivationProfitAmount,
        address _tokenOut,
        address _to
    ) internal returns (uint256 deactivationAmount) {
        uint256 callerB01Balance = b01Token.balanceOf(msg.sender);
        uint256 treasuryFee;
        IERC20 tokenOut = IERC20(_tokenOut);

        if (callerB01Balance < b01Threshold) {
            treasuryFee = (_deactivationProfitAmount * treasuryFeePercentage) / DENOMINATOR;
            tokenOut.safeTransfer(treasury, treasuryFee);
        }
        uint256 feeRecipientFee = (_deactivationProfitAmount * deactivationFeePercentage) /
            DENOMINATOR;
        tokenOut.safeTransfer(feeRecipient, feeRecipientFee);

        deactivationAmount = _deactivationProfitAmount - treasuryFee - feeRecipientFee;
        tokenOut.safeTransfer(_to, deactivationAmount);
    }

    function _getAddLiquidityIn(
        uint256 liquidityOut
    ) private view returns (uint256 amount0, uint256 amount1) {
        IUniswapV2Pair lpAssurance_ = lpAssurance;
        (uint256 reserve0, uint256 reserve1) = _calculateTokensForLiquidity();
        uint256 NAPsupply = lpAssurance_.balanceOf(address(this));
        amount0 = (((liquidityOut * reserve0) / NAPsupply) * 999) / 1000;
        amount1 = (((liquidityOut * reserve1) / NAPsupply) * 999) / 1000;
    }

    function _getActivationData(
        address tokenIn,
        uint256[] calldata tokenIds,
        uint256[] calldata amounts
    ) public view returns (ActivationData memory) {
        require(tokenIds.length == amounts.length, "Mismatched input arrays");
        uint256 totalActivationPrice;

        for (uint256 i; i < tokenIds.length; i++) {
            uint256 proportion = getNFTPrice(tokenIds[i]) * amounts[i];
            totalActivationPrice += proportion;
        }

        if (totalActivationPrice == 0) {
            return
                ActivationData({
                    amountInA: 0,
                    amountInB: 0,
                    path: new address[](0),
                    totalAmountInA: 0
                });
        }

        (uint256 amount0, uint256 amount1) = _getAddLiquidityIn(totalActivationPrice);
        address[] memory path = new address[](2);
        address token0 = lpAssurance.token0();
        address token1 = lpAssurance.token1();
        require(tokenIn == token0 || tokenIn == token1, "Invalid tokenIn");

        path[0] = tokenIn;
        path[1] = (tokenIn == token0) ? token1 : token0;
        uint256 amountInA = (tokenIn == token0) ? amount0 : amount1;
        uint256 amountInB = (tokenIn == token0) ? amount1 : amount0;

        /// Calculate the total amount of `tokenIn` needed including swapping
        uint256[] memory amountsIn = uniswapRouter.getAmountsIn(amountInB, path);
        uint256 totalAmountInA = amountInA + amountsIn[0];

        return
            ActivationData({
                amountInA: amountInA,
                amountInB: amountInB,
                path: path,
                totalAmountInA: totalAmountInA
            });
    }

    function _checkTotalWeight() private view returns (uint256) {
        uint256 totalWeight;
        for (uint256 i; i < allTokenIds.length; i++) {
            totalWeight += percentage[allTokenIds[i]];
        }
        return totalWeight;
    }
