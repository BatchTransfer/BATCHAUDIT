// Chain: POLYGON - File: src/randomness/MasterfileRandomnessProvider.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.20;

import {Ownable} from "@openzeppelin/contracts/access/Ownable.sol";
import {Context} from "@openzeppelin/contracts/utils/Context.sol";
import {ContextUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import {NativeRandomMetaTransaction} from "../utils/NativeRandomMetaTransaction.sol";
import {IMasterfileRandomnessConsumer} from "./MasterfileRandomnessConsumer.i.sol";

enum RequestStatus {
    Uninitialized,
    Pending,
    Fulfilled
}

struct RandomnessRequest {
    address requester;
    RequestStatus status;
}

contract MasterfileRandomnessProvider is Ownable, NativeRandomMetaTransaction {
    event RandomnessRequested(
        uint256 indexed requestId, address indexed requester, address indexed recipient, bytes32 nonce
    );
    event RandomnessFulfilled(uint256 indexed requestId, address indexed requester, uint256 randomness);

    error RepeatedRequest();
    error RequestNotPending();

    mapping(uint256 => RandomnessRequest) public requests;

    constructor(address owner_) Ownable(owner_) initializer {
        __NativeMetaTransaction__init("MASTERFILE_RANDOMNESS_FULFILLER");
    }

    function requestRandomness(address recipient_, bytes32 nonce_) external returns (uint256 requestId) {
        address requester = _msgSender();
        requestId = _computeRequestId(requester, recipient_, nonce_);

        if (requests[requestId].status != RequestStatus.Uninitialized) {
            revert RepeatedRequest();
        }

        requests[requestId].status = RequestStatus.Pending;
        requests[requestId].requester = requester;

        emit RandomnessRequested(requestId, requester, recipient_, nonce_);
    }

    function fulfillRandomness(uint256 requestId, uint256 randomness) external onlyOwner {
        RandomnessRequest storage request = requests[requestId];

        if (request.status != RequestStatus.Pending) {
            revert RequestNotPending();
        }

        request.status = RequestStatus.Fulfilled;

        IMasterfileRandomnessConsumer(request.requester).fulfillRandomness(requestId, randomness);
        emit RandomnessFulfilled(requestId, request.requester, randomness);
    }

    function _computeRequestId(address requester, address recipient, bytes32 nonce) internal pure returns (uint256) {
        return (uint256(keccak256(abi.encode(requester, recipient, nonce))));
    }

    function _msgSender() internal view override(Context, NativeRandomMetaTransaction) returns (address sender) {
        return NativeRandomMetaTransaction._msgSender();
    }

    function _msgData() internal view override(Context, ContextUpgradeable) returns (bytes calldata) {
        return ContextUpgradeable._msgData();
    }
}


