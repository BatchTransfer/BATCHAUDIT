pragma solidity ^0.8.20;

import {ContextUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import {ERC1155Upgradeable} from "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import {OwnableUpgradeable} from "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {Strings} from "@openzeppelin/contracts/utils/Strings.sol";
import {Base64} from "@solady/utils/Base64.sol";
import {NativeMetaTransaction} from "../utils/NativeMetaTransaction.sol";
import {MasterfileRandomnessProvider} from "../randomness/MasterfileRandomnessProvider.sol";

import {
    IMasterfileDrop,
    RandomnessRequest,
    RandomnessRequestStatus,
    MasterfileDropErrors,
    MasterfileDropEvents
} from "../interfaces/IMasterfileDrop.sol";
import {DropDetails, MysteryBox, Bundle, Edition} from "./Types.sol";

/**
 * @dev currently there is a possibility of probability accounting becoming messed up if `numEditions` is reduced in `setDropDetails`
 */
contract MasterfileDrop is OwnableUpgradeable, ERC1155Upgradeable, NativeMetaTransaction, IMasterfileDrop {
    /// @custom:storage-location erc7201:masterfile.storage.MasterfileDrop
    struct MasterfileDropStorage {
        address _usdc;
        address _revenueRecipient;
        address _randomnessProvider;
        uint64 _startDate;
        uint64 _endDate;
        uint256 _mysteryBoxQuantity;
        uint256 _mysteryBoxPrice;
        uint8 _mysteryBoxRedemptions;
        uint256 _bundleQuantity;
        uint256 _bundlePrice;
        uint256[] _bundleTokenIds;
        uint256 _numEditions;
        uint256 _totalProbability;
        bytes32 _arweaveCID;
        mapping(uint256 => Edition) _editions; // tokenId -> Edition
        mapping(address => mapping(bytes32 => bool)) _revealNonceUsed; // TODO: Pack More efficiently
        mapping(uint256 => RandomnessRequest) _randomnessRequests;
    }

    // keccak256(abi.encode(uint256(keccak256("masterfile.storage.MasterfileDrop")) - 1)) & ~bytes32(uint256(0xff))
    bytes32 private constant MasterfileDropStorageLocation =
        0x4df0b4383f38d2cdea820ba306d57260c9c7094a7608b8a831ed1b102434d000;

    function initialize(DropDetails calldata details_) public initializer {
        __Ownable_init(msg.sender);
        __NativeMetaTransaction__init("MasterfileDrop");
        __ERC1155_init("");
        __MasterfileDrop_init(details_);
    }

    function __MasterfileDrop_init(DropDetails calldata details_) internal onlyInitializing {
        _setDropDetails(details_);
    }

    function _getMasterfileDropStorage() private pure returns (MasterfileDropStorage storage $) {
        assembly {
            $.slot := MasterfileDropStorageLocation
        }
    }

    modifier onlyRandomnessProvider() {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        if (msg.sender != $._randomnessProvider) {
            revert InvalidRandomnessResponse();
        }
        _;
    }

    /*//////////////////////////////////////////////////////////////////////////
                                  GETTERS
    //////////////////////////////////////////////////////////////////////////*/

    function getDropWindow() external view returns (uint256 startDate, uint256 endDate) {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        return ($._startDate, $._endDate);
    }

    function getUsdc() external view returns (address) {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        return $._usdc;
    }

    function getRevenueRecipient() external view returns (address) {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        return $._revenueRecipient;
    }

    function getRandomnessProvider() external view returns (address) {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        return $._randomnessProvider;
    }

    function getArweaveCID() external view returns (string memory) {
        return _arweaveCID();
    }

    function getMysteryBox() external view returns (MysteryBox memory) {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        return MysteryBox({
            quantity: $._mysteryBoxQuantity,
            price: $._mysteryBoxPrice,
            redemptions: $._mysteryBoxRedemptions
        });
    }

    function getEdition(uint256 tokenId_) external view returns (Edition memory) {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        return $._editions[tokenId_];
    }

    function getEditions() external view returns (Edition[] memory editions) {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        editions = new Edition[]($._numEditions);

        for (uint256 i = 0; i < $._numEditions; i++) {
            editions[i] = $._editions[i + 1];
        }

        return editions;
    }

    function getBundle() external view returns (Bundle memory bundle) {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        return Bundle({quantity: $._bundleQuantity, price: $._bundlePrice, tokenIds: $._bundleTokenIds});
    }

    function getRedemptionNonceStatus(address user_, bytes32 nonce_) external view returns (bool used) {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        return $._revealNonceUsed[user_][nonce_];
    }

    function getTotalProbability() external view returns (uint256) {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        return $._totalProbability;
    }

    function getRandomnessRequest(uint256 requestId) external view returns (RandomnessRequest memory request) {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        request = $._randomnessRequests[requestId];
    }

    function uri(uint256 tokenId) public view override returns (string memory) {
        string memory baseURI = _baseURI();

        return string.concat(baseURI, _arweaveCID(), "/", Strings.toString(tokenId));
    }

    /*//////////////////////////////////////////////////////////////////////////
                                  ADMIN
    //////////////////////////////////////////////////////////////////////////*/

    function setURI(string memory uri_) external onlyOwner {
        _setURI(uri_);
    }

    function setDropDetails(DropDetails calldata details_) external onlyOwner {
        _setDropDetails(details_);
    }

    function setEdition(uint256 tokenId_, Edition calldata edition_) external onlyOwner {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();

        _assertValidTokenId(tokenId_);

        Edition memory prevEdition = $._editions[tokenId_];

        $._editions[tokenId_] = edition_;

        $._totalProbability += edition_.probability - prevEdition.probability;

        emit EditionSet(tokenId_, edition_.quantity, edition_.price, edition_.probability);
    }

    function setMysteryBox(MysteryBox calldata mysteryBox_) external onlyOwner {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();

        // Must initialize editions before mysteryBox
        if ($._totalProbability == 0) {
            revert InvalidProbabilities();
        }

        $._mysteryBoxQuantity = mysteryBox_.quantity;
        $._mysteryBoxPrice = mysteryBox_.price;
        $._mysteryBoxRedemptions = mysteryBox_.redemptions;

        emit MysteryBoxSet(mysteryBox_.quantity, mysteryBox_.price, mysteryBox_.redemptions);
    }

    function setBundle(Bundle calldata bundle_) external onlyOwner {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();

        $._bundleQuantity = bundle_.quantity;
        $._bundlePrice = bundle_.price;
        $._bundleTokenIds = bundle_.tokenIds;

        for (uint256 i = 0; i < bundle_.tokenIds.length; i++) {
            if (bundle_.tokenIds[i] > $._numEditions) {
                revert InvalidTokenId();
            }
        }

        emit BundleSet(bundle_.quantity, bundle_.price, bundle_.tokenIds);
    }

    /// @notice Mint a token.
    /// @dev Only callable by the owner.
    /// @dev Token Id 0 is reserved for the mystery box.
    /// @dev Token Id X corresponds to edition X - 1.
    /// @param to_ the address to receive the token.
    /// @param tokenId_ the token id to mint.
    /// @param quantity_ the quantity to mint.
    function gift(address to_, uint256 tokenId_, uint256 quantity_) external onlyOwner {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        if (tokenId_ > $._numEditions) {
            revert InvalidTokenId();
        }
        _mint(to_, tokenId_, quantity_, "");
    }

    /// @notice Mint a batch of tokens.
    /// @dev Only callable by the owner.
    /// @dev Token Id 0 is reserved for the mystery box.
    /// @dev Token Id X corresponds to edition X - 1.
    /// @param to_ the address to receive the token.
    /// @param tokenIds_ the token ids to mint.
    /// @param quantities_ the quantity to mint for each token id.
    function giftBatch(address to_, uint256[] calldata tokenIds_, uint256[] calldata quantities_) external onlyOwner {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();

        for (uint256 i = 0; i < tokenIds_.length; i++) {
            if (tokenIds_[i] > $._numEditions) {
                revert InvalidTokenId();
            }
        }

        _mintBatch(to_, tokenIds_, quantities_, "");
    }

    /*//////////////////////////////////////////////////////////////////////////
                                  PURCHASE
    //////////////////////////////////////////////////////////////////////////*/

    function purchaseEdition(
        address recipient_,
        address funder_,
        uint256 tokenId_,
        uint64 quantity_,
        uint256 shippingCost_
    ) external {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();

        /// Checks
        _assertLive();
        _assertValidTokenId(tokenId_);

        Edition storage edition = $._editions[tokenId_];

        if (quantity_ > edition.quantity) {
            revert SoldOut();
        }

        /// Effects

        // Update Quantity
        edition.quantity -= quantity_;

        // Mint Edition
        _mint(recipient_, tokenId_, quantity_, "");

        /// Interactions

        // Transfer Funds
        uint256 totalPrice = edition.price * quantity_ + shippingCost_;

        IERC20($._usdc).transferFrom(funder_, $._revenueRecipient, totalPrice);

        emit EditionPurchased(funder_, recipient_, tokenId_, quantity_, totalPrice);
    }

    function purchaseMysteryBox(address recipient_, address funder_, uint256 quantity_, uint256 shippingCost_)
        external
    {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();

        /// Checks

        _assertLive();

        if (quantity_ > $._mysteryBoxQuantity) {
            revert SoldOut();
        }

        /// Effects

        // Update quantity
        $._mysteryBoxQuantity -= quantity_;

        // Mint Mystery Box
        _mint(recipient_, 0, quantity_, "");

        /// Interactions

        // Transfer Funds
        uint256 totalPrice = $._mysteryBoxPrice * quantity_ + shippingCost_;

        IERC20($._usdc).transferFrom(funder_, $._revenueRecipient, totalPrice);

        emit MysteryBoxPurchased(funder_, recipient_, quantity_, totalPrice);
    }

    function purchaseBundle(address recipient_, address funder_, uint256 quantity_, uint256 shippingCost_) external {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();

        /// Checks

        _assertLive();

        if (quantity_ > $._bundleQuantity) {
            revert SoldOut();
        }

        /// Effects

        // Update quantity
        $._bundleQuantity -= quantity_;

        uint256[] memory tokenIds = $._bundleTokenIds;
        uint256[] memory quantities = new uint256[](tokenIds.length);

        for (uint256 i = 0; i < quantities.length; i++) {
            quantities[i] = 1;
        }

        // Mint Masterfile Plus
        _mintBatch(recipient_, tokenIds, quantities, "");

        /// Interactions

        // Transfer Funds
        uint256 totalPrice = $._bundlePrice * quantity_ + shippingCost_;

        IERC20($._usdc).transferFrom(funder_, $._revenueRecipient, totalPrice);

        emit BundlePurchased(funder_, recipient_, quantity_, totalPrice);
    }

    /*//////////////////////////////////////////////////////////////////////////
                                  REVEAL
    //////////////////////////////////////////////////////////////////////////*/

    // Reveal

    function reveal(bytes32 nonce_) external {
        address recipient = _msgSender();
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();

        /// Checks

        if ($._revealNonceUsed[recipient][nonce_]) {
            revert InvalidNonce();
        }

        // Effects

        _burn(_msgSender(), 0, 1);
        $._revealNonceUsed[recipient][nonce_] = true;

        // Interactions

        uint256 requestId = MasterfileRandomnessProvider($._randomnessProvider).requestRandomness(recipient, nonce_);
        $._randomnessRequests[requestId] =
            RandomnessRequest({recipient: recipient, nonce: nonce_, status: RandomnessRequestStatus.Requested});

        emit MysteryBoxRevealRequested(requestId, recipient, nonce_);
    }

    function fulfillRandomness(uint256 requestId_, uint256 randomness_) external onlyRandomnessProvider {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        RandomnessRequest storage request = $._randomnessRequests[requestId_];
        uint256 numRedemptions = $._mysteryBoxRedemptions;

        /// Checks

        if ($._totalProbability == 0) {
            revert InvalidProbabilities();
        }

        if (request.status != RandomnessRequestStatus.Requested) {
            revert InvalidRandomnessResponse();
        }

        emit MysteryBoxRevealed(requestId_, request.recipient, randomness_);

        uint256[] memory tokenIds = new uint256[](numRedemptions);
        uint256[] memory quantities = new uint256[](numRedemptions);

        // Placeholders
        uint256 randomNumber;
        uint256 cumulativeProbability;
        uint256 tokenId = 1;

        for (uint256 i = 0; i < numRedemptions; i++) {
            randomNumber = uint256(keccak256(abi.encode(randomness_, i))) % $._totalProbability;

            while (cumulativeProbability <= randomNumber) {
                cumulativeProbability += $._editions[tokenId].probability;
                tokenId++;
            }

            // tokenId -1 since incremented on last loop iteration
            tokenId -= 1;

            // add to mint batch
            tokenIds[i] = tokenId;
            quantities[i] = 1;

            emit EditionRedeemed(requestId_, request.recipient, tokenId);

            // reset
            cumulativeProbability = 0;
            tokenId = 1;
        }

        // Effects

        _mintBatch(request.recipient, tokenIds, quantities, "");
        request.status = RandomnessRequestStatus.Fulfilled;
    }

    function _baseURI() internal view virtual returns (string memory) {
        return "https://arweave.net/";
    }

    function _arweaveCID() internal view returns (string memory) {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();
        bytes32 cid = $._arweaveCID;

        return Base64.encode(abi.encodePacked(cid), true, true);
    }

    function _msgSender() internal view override(ContextUpgradeable, NativeMetaTransaction) returns (address sender) {
        return NativeMetaTransaction._msgSender();
    }

    function _assertLive() internal view {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();

        if (block.timestamp < $._startDate) {
            revert DropNotStarted();
        }

        if (block.timestamp > $._endDate) {
            revert DropConcluded();
        }
    }

    function _assertValidTokenId(uint256 tokenId_) internal view {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();

        if (tokenId_ == 0) {
            revert ReservedTokenId();
        }

        if (tokenId_ > $._numEditions) {
            revert InvalidTokenId();
        }
    }

    function _setDropDetails(DropDetails calldata details_) internal {
        MasterfileDropStorage storage $ = _getMasterfileDropStorage();

        if (details_.startDate > details_.endDate) {
            revert InvalidDropWindow();
        }

        $._startDate = details_.startDate;
        $._endDate = details_.endDate;
        $._numEditions = details_.numEditions;
        $._usdc = details_.usdc;
        $._revenueRecipient = details_.revenueRecipient;
        $._randomnessProvider = details_.randomnessProvider;
        $._arweaveCID = details_.arweaveCID;

        // TODO: Clear probabilities between old and new numEditions?

        emit DetailsSet(
            details_.usdc,
            details_.revenueRecipient,
            details_.randomnessProvider,
            details_.startDate,
            details_.endDate,
            details_.numEditions,
            details_.arweaveCID
        );
    }
}


