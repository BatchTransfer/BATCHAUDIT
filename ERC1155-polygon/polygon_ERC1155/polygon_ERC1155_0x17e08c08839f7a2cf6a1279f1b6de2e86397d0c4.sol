     */

    function totalCostForIFOBuy(uint256 id, uint256 amount) public view returns(uint256){
        // f(x) = x * i_p + 2 * 10^-10 * x^3 / 3
        // f(y) - f(x) = (y - x) * i_p + 2 * 10^-10 / 3 * (y^3 - x^3)
        // f(stocksSold + amount) - f(stocksSold)

        // require(stocksSold > 0 && stocksSold < 1000000);
        // require(amount > 0 && amount < 1000000);

        // multiplier = 10 ^ decimals
        // data[id].initialPrice == i_p * multiplier
        // amount * data[id].initialPrice + getCeil(2 * multiplier * ((stocksSold + amount)^3 - stocksSold^3), 3 * 10^10)
        
        return amount * data[id].initialPrice + getCeil(2 * ((data[id].stocksSold + amount)**3 - data[id].stocksSold**3), 3 * 10**(10 - decimals));
    }

    function getCurrentMarketPrice(uint256 id) public view returns (uint256) {
        address pool = getPool[id];

        require(pool != address(0));

        return IFavPool(pool).getPrice();
    }

    function splitAmountsForFee(address pool, uint256 amount, bool flag) external view returns(uint256, uint256){

        return IFavPool(pool).splitAmountsForFee(amount, flag);
    }
    
    function isValidIFOData(uint256 price, uint256 startTime, uint256 endTime, uint256 supplyForIFO) public view returns (bool) {
        // price
        // initial price
        // normally, 1 <= i_p <= 2
        
        return price >= minInitialPrice && price <= maxInitialPrice && endTime > startTime && endTime > block.timestamp && supplyForIFO <= totalSupplyPerFav / 2;
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC1155, ERC1155Holder) returns (bool) {
        return interfaceId == type(IFavStock).interfaceId || super.supportsInterface(interfaceId);
    }

    function getCeil(uint256 a, uint256 b) public pure returns (uint256) {
        return (a + b - 1) / b;
    }
}


