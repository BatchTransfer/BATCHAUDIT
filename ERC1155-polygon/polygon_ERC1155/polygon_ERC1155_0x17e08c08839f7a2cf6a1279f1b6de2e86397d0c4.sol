// Chain: POLYGON - File: contracts/FavStock.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.20;

// import "hardhat/console.sol";

import "@openzeppelin/contracts/access/Ownable.sol";

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";

import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";

import "./interfaces/IFavStock.sol";
import "./interfaces/IFavPoolFactory.sol";
import "./interfaces/IFavPool.sol"; 


contract FavStock is ERC1155, ERC1155Holder, Ownable, IFavStock{

    // Stock price in IFO goes through a bonding curve based on the following formula
    // p(x) = i_p + 2 * 10^-10 * x^2
    // x: number of stocks sold
    // i_p: stock's initial price (1 <= i_p < 2)
    // Therefore, the favDollar amount raised during IFO bonding curve is calculaed as follows
    // f(x) = x * i_p + 2 * 10^-10 * x^3 / 3

    struct IFOData {
        uint256 stocksSold;
        uint256 totalFund;
        uint256 initialPrice;
        uint256 currentPrice;
        uint256 startTime;
        uint256 endTime;
        uint256 supplyForIFO;
    }

    IERC20Metadata public favDollar;
    IFavPoolFactory public favPoolFactory;

    address public override taxWallet;
    address public override limitOrderManager;
    address public override liquidityProvisionManager;
    

    uint256 public totalSupplyPerFav;

    uint256 public override decimals; // favDollar's decimal point
    uint256 public override multiplier; // 10^decimals

    uint256 public override minInitialPrice;
    uint256 public override maxInitialPrice;

    uint256 public override minPriceForDirectLiquidityProvision;
    uint256 public override maxStocksForDirectLiquidityProvision;

    mapping(uint256 => IFOData) public data;
    
    mapping(uint256 => address) public override getPool;
    mapping(address => bool) public override isPool;

    address private _pool;

    modifier afterIFO(uint256 id) {
        require(
            block.timestamp >= data[id].endTime,
            "IFO not finished"
        );
        _;
    }

    modifier inIFO(uint256 id) {
        require(
            block.timestamp >= data[id].startTime &&
                block.timestamp < data[id].endTime,
            "IFO is not ongoing"
        );
        _;
    }

    modifier initialized() {
        require(address(favDollar) != address(0) && address(favPoolFactory) != address(0));
        _;
    }

    constructor() ERC1155("https://favrr.com") Ownable(msg.sender){
    }

    // Function to receive Matic. msg.data must be empty
    receive() external payable {}

    // Fallback function is called when msg.data is not empty
    fallback() external payable {}

    function setMinInitialPrice(uint256 min_) external onlyOwner {
        minInitialPrice = min_;
    }

    function setMaxInitialPrice(uint256 max_) external onlyOwner {
        maxInitialPrice = max_;
    }

    function setMinPriceForDirectLiquidityProvision(uint256 min_) external onlyOwner {
        minPriceForDirectLiquidityProvision = min_;
    }

    function setMaxStocksForDirectLiquidityProvision(uint256 max_) external onlyOwner {
        maxStocksForDirectLiquidityProvision = max_;
    }

    function setTotalSupplyPerFav(uint256 total_) external onlyOwner {
        totalSupplyPerFav = total_;
    }

    function sweepContingency() external onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }

    function sweepERC20Tokens(address token) external onlyOwner {
        IERC20 TOKEN = IERC20(token);
        TOKEN.transfer(owner(), TOKEN.balanceOf(address(this)));
    }

    function sweepERC1155Tokens(address token, uint256 id) external onlyOwner {
        IERC1155 TOKEN = IERC1155(token);
        TOKEN.safeTransferFrom(address(this), owner(), id, TOKEN.balanceOf(address(this), id), "");
    }

    function setFavPoolFactory(address favPoolFactory_) external onlyOwner{
        favPoolFactory = IFavPoolFactory(favPoolFactory_);
    }

    function setFavDollar(address favDollar_) external onlyOwner {
        favDollar = IERC20Metadata(favDollar_);
        decimals = favDollar.decimals();

        require(decimals <= 10);

        multiplier = 10 ** decimals;
    }

    function setTaxWallet(address taxWallet_) external onlyOwner {
        taxWallet = taxWallet_;
    }

    function setLimitOrderManager(address limitOrderManager_) external onlyOwner{
        limitOrderManager = limitOrderManager_;
    }

    function setLiquidityProvisionManager(address liquidityProvisionManager_) external onlyOwner{
        liquidityProvisionManager = liquidityProvisionManager_;
    }

    function buyFromIFO(uint256 id, uint256 amount)
        external
        initialized
        inIFO(id)
    {
        require(data[id].stocksSold + amount <= data[id].supplyForIFO);

        uint256 totalCost = totalCostForIFOBuy(id, amount);

        favDollar.transferFrom(
            msg.sender,
            address(this),
            totalCost
        );

        // Update Fav's IFO information
        data[id].stocksSold += amount;
        data[id].totalFund += totalCost;
        data[id].currentPrice = getCurrentIFOPrice(id); // the price after `amount` stocks are sold

        // Mint ERC1155 FavStock to msg sender
        _mint(msg.sender, id, amount, "");

        emit BuyFromIFO(msg.sender, id, amount);
    }

    function createPool(uint256 id, uint8 fee)
        external
        initialized
        afterIFO(id)
    {
        require(getPool[id] == address(0), "Pool already created");
        require(_msgSender() == liquidityProvisionManager);
        require(fee >= 0 && fee <= 10);
        require(data[id].stocksSold > 0, "No stocks sold");

        uint256 totalFund = data[id].totalFund;
        uint256 currentPrice = data[id].currentPrice;

        // maximum stock amount to put into internal AMM liquidity pool
        uint256 poolStocks = totalFund / currentPrice;

        // maximum favDollar amount to put into internal AMM liquidity pool
        uint256 poolFund = poolStocks * currentPrice;

        uint256 stocksLeft = totalSupplyPerFav - data[id].stocksSold;

        _mint(address(favPoolFactory), id, stocksLeft, "");
        favDollar.transfer(address(favPoolFactory), poolFund);

        uint256 stocks;

        (_pool, stocks) = favPoolFactory.createPool(id, fee, poolStocks, currentPrice, stocksLeft);

        getPool[id] = _pool;
        isPool[_pool] = true;

        favDollar.transfer(taxWallet, totalFund - poolFund);

        emit PoolCreated(id, currentPrice, stocks);
    }

    // Directly create a liquidity pool without going through IFO
    function createPoolWithoutIFO(address liquidityProvider, uint256 id, uint8 fee, uint256 stocks, uint256 price)
        external
        initialized
    {
        require(data[id].initialPrice == 0);
        require(getPool[id] == address(0), "Pool already created");
        require(_msgSender() == liquidityProvisionManager);
        require(fee >= 0 && fee <= 10);
        require(price >= minPriceForDirectLiquidityProvision);
        require(stocks <= maxStocksForDirectLiquidityProvision);

        _mint(address(favPoolFactory), id, totalSupplyPerFav, "");

        _pool = favPoolFactory.createPoolWithoutIFO(liquidityProvider, id, fee, stocks, price, totalSupplyPerFav);

        getPool[id] = _pool;
        isPool[_pool] = true;

        emit PoolCreated(id, price, stocks);
    }

    function marketBuy(uint256 id, uint256 amount) initialized afterIFO(id) external {
        require(amount > 0);

        address pool = getPool[id];
        require(pool != address(0));
        
        IFavPool favPool = IFavPool(pool);

        uint256 reserve0 = favPool.reserve0();
        require(reserve0 > amount);

        uint256 beforePrice = getCurrentMarketPrice(id);
        uint256 cost = favPool.buy(_msgSender(), amount);
        uint256 afterPrice = getCurrentMarketPrice(id);

        emit MarketOrder(_msgSender(), id, amount, beforePrice, afterPrice, cost, OrderType.BUY);
    }

    function marketSell(uint256 id, uint256 amount) initialized afterIFO(id) external{
        require(amount > 0);

        address pool = getPool[id];
        require(pool != address(0));
        IFavPool favPool = IFavPool(pool);

        uint256 beforePrice = getCurrentMarketPrice(id);
        uint256 earnings = favPool.sell(_msgSender(), amount);
        uint256 afterPrice = getCurrentMarketPrice(id);

        emit MarketOrder(_msgSender(), id, amount, beforePrice, afterPrice, earnings, OrderType.SELL);
    }

    function limitBuy(address buyer, uint256 id, uint256 amount) initialized afterIFO(id) external{
        require(_msgSender() == limitOrderManager, "Favrr: Forbidden Call");
        require(amount > 0);

        address pool = getPool[id];
        require(pool != address(0));
        
        IFavPool favPool = IFavPool(pool);

        uint256 reserve0 = favPool.reserve0();
        require(reserve0 > amount);

        uint256 beforePrice = getCurrentMarketPrice(id);
        uint256 cost = favPool.buy(buyer, amount);
        uint256 afterPrice = getCurrentMarketPrice(id);

        emit MarketOrder(buyer, id, amount, beforePrice, afterPrice, cost, OrderType.BUY);
    }

    function limitSell(address seller, uint256 id, uint256 amount) initialized afterIFO(id) external{
        require(_msgSender() == limitOrderManager, "Favrr: Forbidden Call");
        require(amount > 0);

        address pool = getPool[id];
        require(pool != address(0));
        IFavPool favPool = IFavPool(pool);

        uint256 beforePrice = getCurrentMarketPrice(id);
        uint256 earnings = favPool.sell(seller, amount);
        uint256 afterPrice = getCurrentMarketPrice(id);

        emit MarketOrder(seller, id, amount, beforePrice, afterPrice, earnings, OrderType.SELL);
    }

    function createNewIFO(
        uint256 id,
        uint256 price,
        uint256 startTime,
        uint256 endTime,
        uint256 supplyForIFO
    ) external onlyOwner {
        require(data[id].initialPrice == 0, "IFO already created");

        require(isValidIFOData(price, startTime, endTime, supplyForIFO));

        data[id].initialPrice = price;
        data[id].currentPrice = price;
        data[id].startTime = startTime;
        data[id].endTime = endTime;
        data[id].supplyForIFO = supplyForIFO;

        emit IFOCreated(id, price, startTime, endTime, supplyForIFO);
    }

    function removeIFO(uint256 id) external onlyOwner {
        require(data[id].initialPrice != 0);
        require(data[id].stocksSold == 0);

        data[id].initialPrice = 0;
        data[id].currentPrice = 0;
        data[id].startTime = 0;
        data[id].endTime = 0;
        data[id].supplyForIFO = 0;

        emit IFORemoved(id);
    }

    function modifyIFOData(uint256 id, uint256 price, uint256 startTime, uint256 endTime, uint256 supplyForIFO) external onlyOwner{
        require(data[id].initialPrice != 0, "IFO not created yet");
        require(data[id].stocksSold == 0, "Stocks already sold in IFO");
        
        require(isValidIFOData(price, startTime, endTime, supplyForIFO));

        data[id].initialPrice = price;
        data[id].currentPrice = price;
        data[id].startTime = startTime;
        data[id].endTime = endTime;
        data[id].supplyForIFO = supplyForIFO;

        emit IFODataModified(id, price, startTime, endTime, supplyForIFO);
    }

    function changeIFOEndTime(uint256 id, uint256 endTime) external onlyOwner {
        require(data[id].initialPrice != 0, "IFO not created yet");
        require(isValidIFOData(data[id].initialPrice, data[id].startTime, endTime, data[id].supplyForIFO));

        data[id].endTime = endTime;

        emit IFOEndTimeChanged(id, endTime);
    }

    function changeIFOSupply(uint256 id, uint256 supplyForIFO) external onlyOwner {
        require(data[id].initialPrice != 0, "IFO not created yet");
        require(supplyForIFO > data[id].supplyForIFO);
        require(isValidIFOData(data[id].initialPrice, data[id].startTime, data[id].endTime, supplyForIFO));

        data[id].supplyForIFO = supplyForIFO;

        emit IFOSupplyChanged(id, supplyForIFO);
    }

    function getTaxWallet() external view returns (address) {
        return taxWallet;
    }

    function totalCostForMarketBuy(uint256 id, uint256 amount) external view returns (uint256) {
        address pool = getPool[id];

        require(pool != address(0));

        return IFavPool(pool).totalCostForMarketBuy(amount);
    }
    
    function totalEarningsFromMarketSell(uint256 id, uint256 amount) external view returns (uint256) {
        address pool = getPool[id];

        require(pool != address(0));

        return IFavPool(pool).totalEarningsFromMarketSell(amount);
    }

    function getMostRecentPool() external view returns (address) {
        return _pool;
    }

    /**
     * @dev returns p(data[id].stocksSold)
     * p(x) = i_p + 2 * 10^-10 * x^2
     * This is a function to calculate the current price of `id` fav,
     * assuming that `stocksSold` stocks are sold so far
     */

    function getCurrentIFOPrice(uint256 id)
        public
        view
        returns (uint256)
    {
        // p(x) = i_p + 2 * 10^-10 * x^2
        // multiplier = 10 ^ decimals
        // data[id].initialPrice == i_p * multiplier
        // data[id].initialPrice + 2 * multiplier * data[id].stocksSold^2 / 10^10
        return data[id].initialPrice + 2 * data[id].stocksSold ** 2 / 10**(10 - decimals);
    }

