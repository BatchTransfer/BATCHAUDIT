pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "./interfaces/IColorableRegistry.sol";
import "./interfaces/ICrayonStorage.sol";
import "./interfaces/IColorableOwnershipManager.sol";
import "./interfaces/IColorableMetadata.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Receiver.sol";
import "hardhat/console.sol";

contract ColoringManager is AccessControl, ERC1155Receiver {
    event ColoringRequest(
        address indexed collection,
        uint256 indexed tokenId,
        address indexed caller,
        bytes requestId,
        uint256 blockNumber
    );

    event ApplyColor(
        address indexed collection,
        uint256 indexed tokenId,
        address indexed caller,
        bytes requestId,
        uint256 blockNumber
    );

    event CancelColoringRequest(
        address indexed collection,
        uint256 indexed tokenId,
        address indexed caller,
        bytes requestId,
        uint256 blockNumber
    );

    mapping(uint8 => string) public coloringStateMapping;
    mapping(bytes => uint8) public coloringRequestState;
    mapping(uint256 => bytes) public pendingColoringRequests;
    mapping(uint256 => bool) public isColored;

    IColorableOwnershipManager public ownershipManager;
    ICrayonStorage public crayonStorage;
    IColorableRegistry public colorableRegistry;
    bytes32 public constant COLORING_ROLE = keccak256("COLORING_ROLE");
    bool public isColoringEnabled = true;
    bool public recoloringEnabled = false;
    bool public verifyOwnershipInContract = false;

    constructor(
        address ownershipManager_,
        address crayonStorage_,
        address colorableRegistry_
    ) {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(COLORING_ROLE, msg.sender);
        setOwnershipManager(ownershipManager_);
        setCrayonStorage(crayonStorage_);
        setColorableRegistry(colorableRegistry_);

        coloringStateMapping[1] = "pending";
        coloringStateMapping[2] = "fulfilled";
        coloringStateMapping[3] = "cancelled";
    }

    function _depositCrayons(
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory requestId) internal {
            crayonStorage.safeBatchTransferFrom(msg.sender, address(this), ids, amounts, requestId);
    }

    function _withdrawCrayons(
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory requestId) internal {
            crayonStorage.safeBatchTransferFrom(address(this), msg.sender, ids, amounts, requestId);
    }

    function getColoringRequestStates(bytes[] memory requestIds)
        public
        view
        onlyRole(DEFAULT_ADMIN_ROLE)
        returns (uint8[] memory)
    {
        uint256 _numIds = requestIds.length;
        require(_numIds > 0, "ColoringManager#getColoringRequestStates: EMPTY_REQUEST_ID_ARRAY");

        uint8[] memory states = new uint8[](_numIds);
        for (uint256 i = 0; i < _numIds; i++) {
            states[i] = coloringRequestState[requestIds[i]];
        }
        return states;
    }

    function setVerifyOwnershipInContract(bool enabled) public onlyRole(DEFAULT_ADMIN_ROLE) {
        verifyOwnershipInContract = enabled;
    }

    function setOwnershipManager(address ownershipManager_) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            address(ownershipManager_) != address(0x0),
            "ColoringManager#setOwnershipManager: NULL_CONTRACT_ADDRESS"
        );
        ownershipManager = IColorableOwnershipManager(ownershipManager_);
    }

    function setCrayonStorage(address crayonStorage_) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            address(crayonStorage_) != address(0x0),
            "ColoringManager#setCrayonStorage: NULL_CONTRACT_ADDRESS"
        );
        crayonStorage = ICrayonStorage(crayonStorage_);
    }

    function setColorableRegistry(address colorableRegistry_) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            address(colorableRegistry_) != address(0x0),
            "ColoringManager#setColorableRegistry: NULL_CONTRACT_ADDRESS"
        );
        colorableRegistry = IColorableRegistry(colorableRegistry_);
    }

    function setRecoloringEnabled(bool _recoloringEnabled) public onlyRole(DEFAULT_ADMIN_ROLE) {
        recoloringEnabled = _recoloringEnabled;
    }

    function setIsColoringEnabled(bool _isColoringEnabled) public onlyRole(DEFAULT_ADMIN_ROLE) {
        isColoringEnabled = _isColoringEnabled;
    }

    function setIsColored(uint256[] memory _tokenIds, bool[] memory _isColored) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            _tokenIds.length == _isColored.length,
            "ColoringManager#setIsColored: PARAM_LENGTH_MISMATCH"
        );
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            isColored[_tokenIds[i]] = _isColored[i];
        }
    }

    function cancelColoringRequest(bytes memory coloringRequestId) external {
        (address collection, uint256 tokenId, , , uint256[] memory colorIds, address caller, ) = abi.decode(
            coloringRequestId,
            (address, uint256, uint256[], uint256[], uint256[], address, uint256)
        );
        require(coloringRequestState[coloringRequestId] != 2 && coloringRequestState[coloringRequestId] != 3, "ColoringManager#cancelColoringRequest: REQUEST_COMPLETED");
        require(coloringRequestState[coloringRequestId] != 0, "ColoringManager#cancelColoringRequest: REQUEST_NOT_FOUND");

        require(
            caller == msg.sender || hasRole(COLORING_ROLE, msg.sender),
            "ColoringManager#cancelColoringRequest: UNAUTHORIZED"
        );

        coloringRequestState[coloringRequestId] = 3;
        pendingColoringRequests[tokenId] = bytes("");

        uint256 _colorIdsLen = colorIds.length;
        uint256 _numCrayonIds = crayonStorage.numCrayonTypes(); // crayons are indexed from 0
        uint256[] memory _numColorsNeeded = new uint256[](_numCrayonIds); // # needed of color with id i
        for (uint256 i = 0; i < _colorIdsLen; i++) {
            uint256 _colorId = colorIds[i];
            _numColorsNeeded[_colorId]++;
        }

        uint256[] memory _crayonIds = new uint256[](_numCrayonIds);
        for (uint256 c = 0; c < _numCrayonIds; c++) {
            _crayonIds[c] = c;
        }

        _withdrawCrayons(_crayonIds, _numColorsNeeded, coloringRequestId);

        emit CancelColoringRequest(collection, tokenId, caller, coloringRequestId, block.number);
    }

    /**
       @dev users calls this event to emit an event that is listened to by CryptoCrayons to verify request, and apply colours on the user's behave 
     */
    function requestToColor(
        address collection,
        uint256 tokenId,
        uint256[] memory traitIds,
        uint256[] memory areasToColor,
        uint256[] memory colorIds
    ) external {
        _validateColoringRequest(collection, tokenId, traitIds, areasToColor, colorIds);

        require(crayonStorage.isApprovedForAll(msg.sender, address(this)), "ColoringManager#requestToColor: COLORING_MANAGER_NOT_APPROVED");

        // verify crayon ownership & colors exist
        // loop through all colors, check if caller owns enough crayons to perform coloring transaction
        uint256 _loopThrough = colorIds.length;
        uint256 _numCrayonIds = crayonStorage.numCrayonTypes(); // crayons are indexed from 0

        // validate colors and record the # of each color needed for this request
        uint256[] memory numColorsNeeded = new uint256[](_numCrayonIds); // # needed of color with id i
        for (uint256 i = 0; i < _loopThrough; i++) {
            uint256 _colorId = colorIds[i];
            require(_colorId >= 0 && _colorId < _numCrayonIds, "ColoringManager#requestToColor: INVALID COLOR");
            numColorsNeeded[_colorId]++;
        }

        console.log("numColorsNeeded %s: ", numColorsNeeded.length);

        // validate balance of crayons for user
        uint256[] memory _crayonIds = new uint256[](_numCrayonIds);
        for (uint256 c = 0; c < _numCrayonIds; c++) {
            // c is colorId
            uint256 _numColorsNeeded = numColorsNeeded[c];
            _crayonIds[c] = c;
            if (_numColorsNeeded > 0) {
                require(
                    _numColorsNeeded <= crayonStorage.balanceOf(msg.sender, c),
                    "ColoringManager#requestToColor: INSUFFICIENT_CRAYON_BALANCE"
                );
            }
        }

        bytes memory requestId = abi.encode(
            collection,
            tokenId,
            traitIds,
            areasToColor,
            colorIds,
            msg.sender,
            block.number
        );

        _depositCrayons(_crayonIds, numColorsNeeded, requestId);

        coloringRequestState[requestId] = 1; // send request as pending
        pendingColoringRequests[tokenId] = requestId;

        emit ColoringRequest(collection, tokenId, msg.sender, requestId, block.number);
    }

    /**
    @dev apply each colorId to the corresponding index in areasToColor, 
    traitIds, to tokenId in collection
     */
    function colorInCanvas(bytes memory coloringRequestId) external onlyRole(COLORING_ROLE) {
        (address collection, uint256 tokenId, , , , address caller, ) = abi.decode(
            coloringRequestId,
            (address, uint256, uint256[], uint256[], uint256[], address, uint256)
        );
        require(crayonStorage.isApprovedForAll(caller, address(this)), "ColoringManager#requestToColor: COLORING_MANAGER_NOT_APPROVED");

        require(
            coloringRequestState[coloringRequestId] == 1,
            "ColoringManager#colorInCanvas: REQUEST_ID_NOT_FOUND"
        );

        coloringRequestState[coloringRequestId] = 2; // send request as being fulfilled
        isColored[tokenId] = true;
        pendingColoringRequests[tokenId] = bytes("");

        emit ApplyColor(collection, tokenId, caller, coloringRequestId, block.number);
    }

    function _validateColoringRequest(
        address collection,
        uint256 tokenId,
        uint256[] memory traitIds,
        uint256[] memory areasToColor,
        uint256[] memory colorIds
    ) internal {
        require(isColoringEnabled, "ColoringManager#_validateColoringRequest: COLORING_DISABLED");
        require(
            address(colorableRegistry) != address(0x0),
            "ColoringManager#_validateColoringRequest: COLORABLE_REGISTRY_NOT_SET"
        );
        require(
            colorableRegistry.registeredColorableCollections(collection),
            "ColoringManager#_validateColoringRequest: COLLECTION_NOT_REGISTERED"
        );

        if (verifyOwnershipInContract) {
            // verify that caller owns tokenId in collection, also verifies that the token exists
            require(
                ownershipManager.ownerOf(collection, tokenId) == msg.sender,
                "ColoringManager#_validateColoringRequest: UNAUTHORIZED"
            );
        }

        require(keccak256(abi.encodePacked(pendingColoringRequests[tokenId])) == keccak256(abi.encodePacked(bytes(""))), "ColoringManager#_validateColoringRequest: REQUEST_EXISTS");

        require(
            traitIds.length == areasToColor.length && areasToColor.length == colorIds.length,
            "ColoringManager#_validateColoringRequest: PARAM_LENGTH_MISMATCH"
        );

        // verify colorMapping
        IColorableMetadata colorableMetadata = colorableRegistry.collectionMetadata(collection);

        require(
            address(colorableMetadata) != address(0x0),
            "ColoringManager#_validateColoringRequest: COLORABLE_METADATA_NOT_SET"
        );

        // iterate through all colorable areas for this tokenId, and see if there is a coloring assignment for this layer, if not, revert tx
        // get all the traits for this tokenId

        if (!isColored[tokenId]) {
            uint256[] memory traits = colorableMetadata.collectionMetadata(tokenId);
            uint256 traitsLen = traits.length;
            for (uint256 i = 0; i < traitsLen; i++) {
                uint256 _traitId = traits[i];
                uint256 numColorableAreas = colorableMetadata.numColorableAreas(_traitId);
                uint256 _layerId = 0;

                // iterate through all colorableAreas
                while (numColorableAreas > 0) {
                    // find all the colorable layers for this trait
                    // for each colorable layer, check if this layer has a coloring assignment

                    bool _isLayerColorable = colorableMetadata.colorableAreas(_traitId, _layerId);
                    if (_isLayerColorable) {
                        // make there is is an index where the traitId and the areasToColor match for this colorable layer
                        uint256 _traitIdsLength = traitIds.length;
                        bool _isFound = false;
                        for (uint256 j = 0; j < _traitIdsLength; j++) {
                            if (traitIds[j] == _traitId && areasToColor[j] == _layerId) {
                                _isFound = true;
                                break;
                            }
                        }

                        require(
                            _isFound,
                            "ColoringManager#_validateColoringRequest: ALL_COLORABLE_AREAS_MUST_BE_COLORED"
                        );

                        numColorableAreas--;
                    }
                    _layerId++;
                }
            }
        }

        // ensure that the nft has not already been colored, and that there is no pending request
        if (!recoloringEnabled) {
            require(
                !isColored[tokenId],
                "ColoringManager#_validateColoringRequest: TOKEN_ALREADY_COLORED"
            );
        }

        colorableMetadata.verifyColorMap(traitIds, areasToColor);
    }

    function onERC1155Received(
        address,
        address,
        uint256,
        uint256,
        bytes memory
    ) public virtual override returns (bytes4) {
        return this.onERC1155Received.selector;
    }

    function onERC1155BatchReceived(
        address,
        address,
        uint256[] memory,
        uint256[] memory,
        bytes memory
    ) public virtual override returns (bytes4) {
        return this.onERC1155BatchReceived.selector;
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(AccessControl, ERC1155Receiver) returns (bool) {
        return AccessControl.supportsInterface(interfaceId) || ERC1155Receiver.supportsInterface(interfaceId);
    }
}


