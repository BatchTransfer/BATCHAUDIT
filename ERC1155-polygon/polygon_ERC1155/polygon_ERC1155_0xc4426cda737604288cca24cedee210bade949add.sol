// Chain: POLYGON - File: /contracts/RewardsJaguarStar.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.23;

import "@openzeppelin/contracts/access/extensions/AccessControlDefaultAdminRules.sol";
import "@openzeppelin/contracts/token/common/ERC2981.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "operator-filter-registry/src/DefaultOperatorFilterer.sol"; // OpenSea Creator Fee Enforcement

/**
 * @title RewardsJaguarStar
 * @author Mainbot
 *
 * @notice Only Mainbot can use this contract to mint RewardsJaguarStar NFTs.
 * @dev ERC1155 contract that has create and mint functionality, and supports useful standards from OpenZeppelin,
 * like _exists(), name(), symbol()
 */

contract RewardsJaguarStar is ERC1155, ERC2981, AccessControlDefaultAdminRules, DefaultOperatorFilterer {
    
    string public name;
    string public symbol;
    string public url;

    struct JaguarStar {
        uint256 id;
        string uri;
        uint256 totalSupply;
    }
    
    mapping (uint256 => JaguarStar) private jaguarStars;

    constructor(string memory _name, string memory _symbol, string memory _url, address _receiver)
    AccessControlDefaultAdminRules(3 hours, msg.sender)
    ERC1155("") {
        name = _name;
        symbol = _symbol;
        url = _url;
        _setDefaultRoyalty(_receiver , 100);
    }

    function createJaguarStars(uint256[] calldata _ids, string[] calldata _uris)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(_ids.length == _uris.length, "RewardsJaguarStar: _ids.length shall equal _uris.length");
        for (uint256 i = 0; i < _ids.length; i++) {
            _createJaguarStar(_ids[i], _uris[i]);
        }
    }

    function mintBatch(
        address[] calldata _tos,
        uint256[] calldata _ids
    )
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(_tos.length == _ids.length, "RewardsJaguarStar: _tos and _ids length mismatch");
        for (uint256 i = 0; i < _tos.length; i++) {
            _beforeMint(_ids[i]);
            _mint(_tos[i], _ids[i], 1, '');
        }
    }

    function setUrl(string memory _url) external onlyRole(DEFAULT_ADMIN_ROLE) {
        url = _url;
    }

    function setUri(uint256 _id, string calldata _uri) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(jaguarStars[_id].id != 0, "RewardsJaguarStar: JaguarStar shall exist");
        jaguarStars[_id].uri = _uri;
    }

    function uri(uint256 _id) public view override returns (string memory) {
       return string.concat(url, jaguarStars[_id].uri);
    }

    function setJaguarStarSupply(uint256 _id, uint256 _totalSupply) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(jaguarStars[_id].id != 0, "RewardsJaguarStar: JaguarStar shall exist");
        jaguarStars[_id].totalSupply = _totalSupply;
    }

    function jaguarStarSupply(uint256 _id) external view returns (uint256) {
        return jaguarStars[_id].totalSupply;
    }

    function jaguarStarExist(uint256 _id) external view returns (bool) {
        return jaguarStars[_id].id != 0;
    }

    function _beforeMint(uint256 _id) internal {
        require(msg.sender != address(0), "RewardsJaguarStar: mint to the zero address");
        require(jaguarStars[_id].id != 0, "RewardsJaguarStar: JaguarStar shall exist");
        jaguarStars[_id].totalSupply += 1;
    }

    function _createJaguarStar(uint256 _id, string calldata _uri) internal {
        require(_id > 0, "RewardsJaguarStar: invalid JaguarStar ID");
        require(jaguarStars[_id].id == 0, "RewardsJaguarStar: JaguarStar already exists");
        jaguarStars[_id] = JaguarStar(_id, _uri, 0);
    }

    // Mandatory OpenSea overridings to allow creator fee enforcement (see https://github.com/ProjectOpenSea/operator-filter-registry#creator-fee-enforcement )
    function setApprovalForAll(address operator, bool approved) public override onlyAllowedOperatorApproval(operator) {
        super.setApprovalForAll(operator, approved);
    }

    function safeTransferFrom(address from, address to, uint256 tokenId, uint256 amount, bytes memory data) public override onlyAllowedOperator(from) {
        super.safeTransferFrom(from, to, tokenId, amount, data);
    }

    // ERC2980 Royalties
    function setDefaultRoyalty(address _receiver, uint96 _feeNumerator) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _setDefaultRoyalty(_receiver, _feeNumerator);
    }

    function setJaguarStarRoyalty(uint256 _id, address _receiver, uint96 _feeNumerator) public onlyRole(DEFAULT_ADMIN_ROLE) {
        require(jaguarStars[_id].id != 0, "RewardsJaguarStar: JaguarStar shall exist");
        _setTokenRoyalty(_id, _receiver, _feeNumerator);
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC1155, ERC2981, AccessControlDefaultAdminRules) returns (bool) {
        return interfaceId == type(IERC1155).interfaceId 
        || interfaceId == type(IERC1155MetadataURI).interfaceId
        || interfaceId == type(IERC2981).interfaceId  
        || super.supportsInterface(interfaceId);
    }

    function withdraw(address[] calldata _addresses) external onlyRole(DEFAULT_ADMIN_ROLE) {
        bool noFunds = true;
        for (uint256 i = 0; i < _addresses.length; i++) {
            if (_addresses[i] == address(0)) {
                uint256 ethBalance = address(this).balance;
                if (ethBalance > 0) {
                    payable(msg.sender).transfer(ethBalance);
                    noFunds = false;
                }
            } else {
                uint256 erc20Balance = IERC20(_addresses[i]).balanceOf(address(this));
                if (erc20Balance > 0) {
                    IERC20(_addresses[i]).transfer(msg.sender, erc20Balance);
                    noFunds = false;
                }
            }
        }
        require(!noFunds, "Nothing to withdraw");
    }
}

