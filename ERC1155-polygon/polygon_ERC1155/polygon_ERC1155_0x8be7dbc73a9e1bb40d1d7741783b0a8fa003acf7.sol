    // address => id => amount
    mapping (address => mapping (uint256 => uint256)) public salesLog;

    // address => id => amount
    mapping (address => mapping (uint256 => uint256)) public claimableTokens;

    EnumerableSet.UintSet private tradableTokens;

    constructor(address arayaNftContractAddress, address fearContractAddress, string memory domainName) {
        arayaNftContract = IArayaNft(arayaNftContractAddress);
        fearContract = IERC20(fearContractAddress);
        _initializeEIP712(domainName);
    }

    function _msgSender() internal override view returns (address sender) {
        return ContextMixin.msgSender();
    }

    function _beforeUnpause() internal view virtual override {
        require(arayaNftContract.hasRole(arayaNftContract.MINTER_ROLE(), address(this)), "Minter role missing");
    }

    function getTradableTokens() external view returns (uint256[] memory) {
        return tradableTokens.values();
    }

    /**
    * @dev Allows caller to buy Araya NFTs using FEAR.
    * @param id Token type.
    * @param amount Amount of tokens to buy.
    * Requirements:
    * - Contract must be unpaused;
    * - Caller must be a wallet address and not a contract;
    * - Price for given token type must be set;
    * - Amount must be greater than zero;
    * - Amount must not exceed token minting limit;
    * - Caller's account must have enough FEAR to buy specified amount of NFTs;
    * - Contract must be approved to transfer enough amount of required assets.
    * Emits {TokenBought} event on success.
    */
    function buyTokens(uint256 id, uint256 amount) public whenNotPaused nonReentrant {
        address caller = _msgSender();
        uint256 tokenPrice = price[id];
        uint256 fullAmount = tokenPrice.mul(amount);

        require(tokenPrice > 0, "Unknown token price");
        require(amount > 0, "Cannot buy zero tokens");
        require(amount <= arayaNftContract.mintingLimits(id) - arayaNftContract.mintedCounts(id), "Amount exceeds token minting limit.");
        require(amount + salesLog[caller][id] <= walletLimit[id] || walletLimit[id] == 0, "Total amount exceeds token limit per wallet.");

        uint256 userBalance = fearContract.balanceOf(caller);
        uint256 allowance = fearContract.allowance(caller, address(this));

        require(userBalance >= fullAmount, "Insufficient balance");
        require(allowance >= fullAmount, "Insufficient allowance");

        salesLog[caller][id] += amount;

        fearContract.transferFrom(caller, address(this), fullAmount);
        arayaNftContract.initialMint(caller, id, amount, "");

        emit TokenBought(caller, id, amount, tokenPrice);
    }

    /**
    * @dev Allows to claim tokens if caller is eligible for that.
    * @param originalAddress Original address.
    * @param fearWalletAddress Address for minting claimed tokens.
    * @param id Token type.
    * @param signature CLAIMING_MESSAGE string signed by originalAddress account.
    * Requirements:
    * - Contract must be unpaused;
    * - Tokens of specified type must be available to claim by originalAddress account;
    * - Signature must be valid.
    * Emits {TokenClaimed} event on success.
    */
    function claimTokens(address originalAddress, address fearWalletAddress, uint256 id, bytes memory signature) external whenNotPaused nonReentrant {
        uint256 amount = claimableTokens[originalAddress][id];
        require(amount > 0, "No tokens available to claim");

        bytes32 messageHash = ECDSA.toEthSignedMessageHash(abi.encodePacked(CLAIMING_MESSAGE));
        bool validSignature = SignatureChecker.isValidSignatureNow(originalAddress, messageHash, signature);
        require(validSignature, "Signature check failure");
        
        claimableTokens[originalAddress][id] = 0;
        arayaNftContract.initialMint(fearWalletAddress, id, amount, "");
        emit TokenClaimed(originalAddress, fearWalletAddress, id, amount);
    }


    //// ADMIN SETUP FUNCTIONS

    function setPrice(uint256 id, uint256 tokenPrice, uint256 limitPerWallet) external onlyRole(MANAGER_ROLE) {
        price[id] = tokenPrice;
        walletLimit[id] = limitPerWallet;

        if (tokenPrice > 0)
            tradableTokens.add(id);
        else
            tradableTokens.remove(id);

        emit PricesChanged(id, tokenPrice);
    }

    function setClaimableTokens(address holderAddress, uint256 id, uint256 amount) external onlyRole(MANAGER_ROLE) {
        claimableTokens[holderAddress][id] = amount;
    }
}