pragma solidity ^0.8.19;

import "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155SupplyUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/utils/ERC1155HolderUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

import "../common/CollectionModel.sol";
import "../interfaces/ICollectionRepository.sol";
import "../interfaces/IAirdropSupplier.sol";
import "../utils/IssueHelper.sol";

/**
 * @notice Contract module which provide a definition of the Polish Crypto Stamp Special Edition 
 * - Battle of Khotyn collection.
 *
 * @custom:website https://nft.poczta-polska.pl
 * @author rutilicus.eth (ArchXS)
 * @custom:security-contact contact@archxs.com
 */
contract PolishCryptoStampSEBoK is Initializable, ContextMixin, ERC1155Upgradeable, AccessControlUpgradeable, ReentrancyGuardUpgradeable, ERC1155BurnableUpgradeable, ERC1155SupplyUpgradeable, ERC1155HolderUpgradeable, IAirdropSupplier, UUPSUpgradeable {
    /**
     * @dev Emitted when `contract` mints item tokens of specific series.
     * @notice The amont of tokens minted is attached to the event.
     */
    event ItemsDispatched(uint256 issueId, uint8 tier, uint256 amount, ItemType itemType, address indexed recipient);
    
    /**
     * @dev Emitted when `contract` issues items tokens of specific collection and category indicated by `baseId`.
     * @notice The size of an issuance is attached to the event.
     */
    event ItemsIssued(uint256 issueId, uint8 tier, ItemType itemType, uint256 tokenId, address indexed recipient);

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    bytes32 public constant CLAIMER_ROLE = keccak256("CLAIMER_ROLE");

    // Mapping to indicate supported issues
    mapping(uint256 => bool) public supportedIssues;
    ItemType public allowedItemType;

    // Holds the OpenSea like contract metadata for the collection
    string private _contractUri;
    string public uriSuffix;

    // Mapping from issue ID to onchain minted tokens per tier counter
    mapping(uint256 => mapping(uint8 => uint256)) private _itemsCounters;
    // Mapping from account to issue series per wallet counter 
    mapping(address => mapping(uint256 => uint256)) private _itemsCollected;

    ICollectionRepository private collectionRepository;
    IssuePhase private _mintingPhase;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    /**
     * The contract constructor.
     * 
     * @param initialUri The base/initial token uri.
     * @param contractUri The uri for OpenSea related collection metadata.
     * @param issueId The default supported issue Id.
     * @param repositoryContract The configuration repository contract address
     */
    function initialize(string calldata initialUri, string memory contractUri, uint256 issueId, address repositoryContract) initializer public {
        __ERC1155_init(initialUri);
        __AccessControl_init();
        __ReentrancyGuard_init();
        __ERC1155Burnable_init();
        __ERC1155Supply_init();
        __UUPSUpgradeable_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(ADMIN_ROLE, msg.sender);
        _grantRole(CLAIMER_ROLE, msg.sender);

        __PolishCryptoStampSEBoK_init_unchained(contractUri, ItemType.Stamp, issueId, repositoryContract);
    }
    
    function __PolishCryptoStampSEBoK_init_unchained(string memory contractUri, ItemType itemType, uint256 issueId, address repositoryContract) internal onlyInitializing {
        uriSuffix = ".json";
        _contractUri = contractUri;
        
        allowedItemType = itemType;
        supportedIssues[issueId] = true;
        _mintingPhase = IssuePhase.General;

        collectionRepository = ICollectionRepository(repositoryContract);
    }

    /**
     * @dev Allows to mint items' NFTs of a specific token to a number of pre-generated paper wallets.
     * @notice This operation is for carefull usage with only fully prepared collection off-chain data in order to lower gas fees.
     */
    function issueItems(uint256 issueId, uint8 tier, Depositary[] calldata depositaries) external onlyPrivileged {
        require(depositaries.length > 0, "Invalid call data");
        require(depositaries[0].offset > _itemsCounters[issueId][tier], "Invalid offset");

        for (uint256 i = 0; i < depositaries.length; i++) {
            _mint(depositaries[i].wallet, depositaries[i].serial, 1, "");
        }

        unchecked {
            _itemsCounters[issueId][tier] += depositaries.length;
        }
    }

    function dropItems(uint256 issueId, uint8 tier, uint256 amount, address recipient) external nonReentrant onlyRole(CLAIMER_ROLE) returns (bool) {
        require(supportedIssues[issueId], "Issue not supported");
        (IssueInfo memory issueInfo, ItemTier memory itemTier) = collectionRepository.getItemInfo(allowedItemType, issueId, tier);

        require(itemTier.maxSupply > _itemsCounters[issueInfo.issueId][itemTier.tier] + amount, "Not enough tokens");
        require(_itemsCollected[recipient][itemTier.tier] + amount <= itemTier.maxMints, "Limit claimed");

        uint256[] memory _ids = new uint256[](amount); 
        uint256[] memory _amounts = new uint256[](amount); 

        for (uint256 i = 0;  i < amount; i++) {
            uint256 _serialId =  _itemsCounters[issueId][tier] + i + 1;
            uint256 _baseId = IssueHelper.combineTokenId(issueId, tier);
            _ids[i] = IssueHelper.combineTokenId(_baseId, _serialId);
            _amounts[i] = 1;
        }

        require(amount == _ids.length, "Limit claimed");
        _dispatchItems(issueId, tier, recipient, _ids, _amounts);

        return true;
    }

    /**
     * @notice Items minting function for the on-chain purchases.
     * 
     * @param issueId The ID of the issue to mint.
     * @param tier The tier of the issue items to mint.
     * @param amount The amount of items to mint.
     * @param recipient If set, the destination wallet. 
     * @param rounds IDs of exchange rate quotation rounds, [0,0] for latest.
     * @param consentApproved By setting the value to `true` you agree to the Terms and conditions of the service.
     */
    function mintItems(uint256 issueId, uint8 tier, uint256 amount, address recipient, uint80[2] calldata rounds, bool consentApproved) public payable virtual nonContract {
        require(consentApproved, "Terms not approved");

        (IssueInfo memory _issueInfo, ItemTier memory _itemTier) = collectionRepository.getItemInfo(allowedItemType, issueId, tier);
        
        require(supportedIssues[issueId] && _issueInfo.issueId > 0, "Invalid series");
        require(_itemTier.isMintable && (_issueInfo.issuePhase == _mintingPhase), "Sale not active!"); 
        require(_itemTier.maxSupply > _itemsCounters[_issueInfo.issueId][_itemTier.tier] + amount, "Not enough tokens");

        address _recipient = (recipient != address(0) ? recipient : _msgSender());
        require(_itemsCollected[_msgSender()][_itemTier.tier] + amount <= _itemTier.maxMints, "Limit claimed");

        uint256[] memory _ids = new uint256[](amount); 
        uint256[] memory _amounts = new uint256[](amount); 

        for (uint256 i = 0;  i < amount; i++) {
            uint256 _serialId =  _itemsCounters[issueId][tier] + i + 1;
            uint256 _baseId = IssueHelper.combineTokenId(issueId, tier);
            _ids[i] = IssueHelper.combineTokenId(_baseId, _serialId);
            _amounts[i] = 1;
        }

        require(amount == _ids.length, "Invalid amount");

        (uint256 _tokenPrice,) = collectionRepository.getDerivedItemPrice(allowedItemType, _issueInfo.issueId, _itemTier.tier, rounds);
        _checkout(_issueInfo.beneficiary, (_tokenPrice * amount));
        _dispatchItems(issueId, tier, recipient, _ids, _amounts);

        emit ItemsDispatched(issueId, tier, amount, allowedItemType, _recipient);      
    }

    /**
     * @notice OpenSea related metadata of the smart contract.
     * @param contractUri Storefront-level metadata for contract.
     */
    function setContractURI(string memory contractUri) external onlyPrivileged {
        _contractUri = contractUri;
    }

    /**
     * @notice OpenSea related metadata of the smart contract.
     * @return Storefront-level metadata for contract.
     */
    function contractURI() public view returns (string memory) {
        return _contractUri;
    }

    function setURI(string memory newuri) public onlyPrivileged {
        _setURI(newuri);
    }

    /**
     * @dev Optional possibility to set up URI suffix for IPFS gateways flexibility.
     */
    function setUriSuffix(string memory suffix) public onlyPrivileged {
        uriSuffix = suffix;
    }

    /**
     * @dev See {IERC1155MetadataURI-uri}.
     */
    function uri(uint256 tokenId) public view virtual override returns (string memory) {
        return _resolveUri(tokenId);
    }

    /**
     * @notice Allows to specify which phase allows open minting.
     * @param phase The phase to set as minting one.
     */
    function setMintingPhase(IssuePhase phase) external onlyPrivileged {
        _mintingPhase = phase;
    }

    /**
     * @notice Allows to setup a new collection repository contract.
     * @param repositoryContract A address to set for the new repository contract.
     */
    function setCollectionRepository(address repositoryContract) external onlyPrivileged {
        require(repositoryContract != address(0) && AddressUpgradeable.isContract(repositoryContract), "Invalid address");
        collectionRepository = ICollectionRepository(repositoryContract);
    }

    /**
     * @dev Allows to set suppored series ID of the existing collection.
     *
     * @param issueIds - Supported issue IDs.
     * @param approvals - Whether allow or disallow the issues.
     */
    function supportIssues(uint256[] calldata issueIds, bool[] calldata approvals) public onlyPrivileged {
        require(issueIds.length == approvals.length, "Invalid data");

        for (uint256 i = 0;  i < issueIds.length; i++) {
            IssueInfo memory issue = collectionRepository.getIssueInfo(allowedItemType, issueIds[i]);
            require(issue.issueId > 0, "Invalid issue");

            supportedIssues[issueIds[i]] = approvals[i];
        }
    }

    /**
     * @dev Allows to reset current counters state
     */
    function resetCounters(uint256 issueId, uint8 tier, uint256 value) external onlyPrivileged {
        _itemsCounters[issueId][tier] = value;
    }

    /**
     * @dev Returns total amount of tokens issued within given issue and tier.
     */
    function totalItemSupply(uint256 issueId, uint8 tier) public view returns (uint256) {
        return _itemsCounters[issueId][tier];
    }

    /**
     * @dev See {IAirdropSupplier-collectedOf}.
     */
    function collectedOf(address account, uint256, uint8 tier) public view virtual returns (uint256) {
        return _itemsCollected[account][tier];
    }

    // ************************************* 
    // Utility methods
    // *************************************

    function _resolveUri(uint256 tokenId) internal view returns (string memory) {
        (uint256 _baseId,) = IssueHelper.extractTokenIds(tokenId);
        (uint256 _issueId, uint256 _tier) = IssueHelper.extractTokenIds(_baseId);

        (IssueInfo memory issue, ItemTier memory tier) = collectionRepository.getItemInfo(allowedItemType, _issueId, uint8(_tier));
        if (block.timestamp <= issue.issueTimestamp) {
            return super.uri(tokenId);
        } 

        bytes memory _bytesUri = bytes(tier.tierUri);

        if (_bytesUri.length > 0 && _bytesUri[_bytesUri.length - 1] == SLASH) {
            string memory _tokenUri = string(abi.encodePacked(tier.tierUri, StringsUpgradeable.toString(_tier)));

            if (bytes(uriSuffix).length > 0) {
                return string(abi.encodePacked(_tokenUri, uriSuffix));
            }
            return _tokenUri;
        } else {
            return tier.tierUri;
        }
    }

    function _dispatchItems(uint256 issueId, uint8 tier, address recipient, uint256[] memory ids, uint256[] memory amounts) internal {
    }

    function _checkout(address beneficiary, uint256 totalAmount) internal {
        require(msg.value >= totalAmount, "Not enough value sent");

        payable(beneficiary).transfer(totalAmount);
        payable(_msgSender()).transfer(msg.value - totalAmount);
    }

    // *************************************
    // Modifiers
    // ************************************* 

    /**
     * @dev Throws if called by a contract
     */
    modifier nonContract() {
        require(tx.origin == _msgSender(), "Call forbidden");
        _;
    }

    /**
     * @dev Throws if called by any account other than the priviledged ones.
     */
    modifier onlyPrivileged() {
        _checkPrivileged();
        _;
    }

    /**
     * @dev Throws if the sender is not the owner.
     */
    function _checkPrivileged() internal view virtual {
        address _sender = _msgSender();
        require(hasRole(DEFAULT_ADMIN_ROLE, _sender) || hasRole(ADMIN_ROLE, _sender), "Caller is not privileged");
    }


    function onERC1155Received(
        address,
        address,
        uint256,
        uint256,
        bytes memory
    ) public virtual override returns (bytes4) {
        // require(operator != address(this), "Sending not allowed");
        revert("Transfer not yet allowed");

        // return this.onERC1155Received.selector;
    }

    function onERC1155BatchReceived(
        address,
        address,
        uint256[] memory,
        uint256[] memory,
        bytes memory
    ) public virtual override returns (bytes4) {
        revert("Batch transfer not allowed");
    }

    /**
   * Override isApprovedForAll to auto-approve OS's proxy contract
   */
    function isApprovedForAll(
        address _owner,
        address _operator
    ) public override view returns (bool isOperator) {
        // if OpenSea's ERC1155 Proxy Address is detected, auto-return true
        // for Polygon's Mumbai testnet, use 0x53d791f18155C211FF8b58671d0f7E9b50E596ad
       if (_operator == address(0x207Fa8Df3a17D96Ca7EA4f2893fcdCb78a304101)) {
            return true;
        }
        // otherwise, use the default ERC1155.isApprovedForAll()
        return ERC1155Upgradeable.isApprovedForAll(_owner, _operator);
    }

    /**
     * This is used instead of msg.sender as transactions won't be sent by the original token owner, but by OpenSea.
     */
    function _msgSender()
        internal
        override
        view
        returns (address sender)
    {
        return ContextMixin.msgSender();
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        onlyPrivileged
        override
    {}

    // The following functions are overrides required by Solidity.

    function _beforeTokenTransfer(address operator, address from, address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data)
        internal
        override(ERC1155Upgradeable, ERC1155SupplyUpgradeable)
    {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(AccessControlUpgradeable, ERC1155ReceiverUpgradeable, ERC1155Upgradeable)
        returns (bool)
    {
        return (ERC1155ReceiverUpgradeable.supportsInterface(interfaceId) ||
            super.supportsInterface(interfaceId));
    }

    /**
     * @dev This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
     */
    uint256[50] private __gap;
}

